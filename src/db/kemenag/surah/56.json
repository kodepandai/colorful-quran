[
	{
		"aya_id": 4980,
		"aya_number": 1,
		"aya_text": "اِذَا وَقَعَتِ الْوَاقِعَةُۙ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "Apabila terjadi hari Kiamat,",
		"tajweed": []
	},
	{
		"aya_id": 4981,
		"aya_number": 2,
		"aya_text": "لَيْسَ لِوَقْعَتِهَا كَاذِبَةٌ  ۘ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "terjadinya tidak dapat didustakan (disangkal).",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 11,
				"end": 12
			}
		]
	},
	{
		"aya_id": 4982,
		"aya_number": 3,
		"aya_text": "خَافِضَةٌ رَّافِعَةٌ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "(Kejadian itu) merendahkan (satu golongan) dan meninggikan (golongan yang lain).",
		"tajweed": [
			{
				"class": "idgham-bilaghunnah",
				"start": 7,
				"end": 12
			}
		]
	},
	{
		"aya_id": 4983,
		"aya_number": 4,
		"aya_text": "اِذَا رُجَّتِ الْاَرْضُ رَجًّاۙ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "Apabila bumi diguncangkan sedahsyat-dahsyatnya,",
		"tajweed": []
	},
	{
		"aya_id": 4984,
		"aya_number": 5,
		"aya_text": "وَّبُسَّتِ الْجِبَالُ بَسًّاۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "dan gunung-gunung dihancurluluhkan sehancur-hancurnya,",
		"tajweed": []
	},
	{
		"aya_id": 4985,
		"aya_number": 6,
		"aya_text": "فَكَانَتْ هَبَاۤءً مُّنْۢبَثًّاۙ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "maka jadilah ia debu yang beterbangan,",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 14,
				"end": 16
			},
			{
				"class": "idgham-bighunnah",
				"start": 16,
				"end": 21
			},
			{
				"class": "iqlab",
				"start": 22,
				"end": 24
			},
			{
				"class": "iqlab",
				"start": 24,
				"end": 26
			}
		]
	},
	{
		"aya_id": 4986,
		"aya_number": 7,
		"aya_text": "وَّكُنْتُمْ اَزْوَاجًا ثَلٰثَةً ۗ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "dan kamu menjadi tiga golongan,",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 5,
				"end": 9
			},
			{
				"class": "ikhfa",
				"start": 19,
				"end": 21
			},
			{
				"class": "ikhfa",
				"start": 22,
				"end": 24
			}
		]
	},
	{
		"aya_id": 4987,
		"aya_number": 8,
		"aya_text": "فَاَصْحٰبُ الْمَيْمَنَةِ ەۙ مَآ اَصْحٰبُ الْمَيْمَنَةِ ۗ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "yaitu golongan kanan, alangkah mulianya golongan kanan itu,",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 30,
				"end": 33
			}
		]
	},
	{
		"aya_id": 4988,
		"aya_number": 9,
		"aya_text": "وَاَصْحٰبُ الْمَشْـَٔمَةِ ەۙ مَآ اَصْحٰبُ الْمَشْـَٔمَةِ ۗ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "dan golongan kiri, alangkah sengsaranya golongan kiri itu,",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 31,
				"end": 34
			}
		]
	},
	{
		"aya_id": 4989,
		"aya_number": 10,
		"aya_text": "وَالسّٰبِقُوْنَ السّٰبِقُوْنَۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "dan orang-orang yang paling dahulu (beriman), merekalah yang paling dahulu (masuk surga).",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 25,
				"end": 27
			}
		]
	},
	{
		"aya_id": 4990,
		"aya_number": 11,
		"aya_text": "اُولٰۤىِٕكَ الْمُقَرَّبُوْنَۚ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "Mereka itulah orang yang dekat (kepada Allah),",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 3,
				"end": 7
			},
			{
				"class": "madd-arid-lissukun",
				"start": 24,
				"end": 26
			}
		]
	},
	{
		"aya_id": 4991,
		"aya_number": 12,
		"aya_text": "فِيْ جَنّٰتِ النَّعِيْمِ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "Berada dalam surga kenikmatan,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 7,
				"end": 9
			},
			{
				"class": "ghunnah",
				"start": 15,
				"end": 17
			},
			{
				"class": "madd-arid-lissukun",
				"start": 20,
				"end": 22
			}
		]
	},
	{
		"aya_id": 4992,
		"aya_number": 13,
		"aya_text": "ثُلَّةٌ مِّنَ الْاَوَّلِيْنَۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "segolongan besar dari orang-orang yang terdahulu,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 5,
				"end": 10
			},
			{
				"class": "madd-arid-lissukun",
				"start": 24,
				"end": 26
			}
		]
	},
	{
		"aya_id": 4993,
		"aya_number": 14,
		"aya_text": "وَقَلِيْلٌ مِّنَ الْاٰخِرِيْنَۗ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "dan segolongan kecil dari orang-orang yang kemudian.",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 8,
				"end": 13
			},
			{
				"class": "madd-arid-lissukun",
				"start": 26,
				"end": 28
			}
		]
	},
	{
		"aya_id": 4994,
		"aya_number": 15,
		"aya_text": "عَلٰى سُرُرٍ مَّوْضُوْنَةٍۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "Mereka berada di atas dipan-dipan yang bertahtakan emas dan permata,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 10,
				"end": 15
			},
			{
				"class": "madd-arid-lissukun",
				"start": 20,
				"end": 22
			}
		]
	},
	{
		"aya_id": 4995,
		"aya_number": 16,
		"aya_text": "مُّتَّكِـِٕيْنَ عَلَيْهَا مُتَقٰبِلِيْنَ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 534,
		"translation_aya_text": "mereka bersandar di atasnya berhadap-hadapan.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 0,
				"end": 2
			},
			{
				"class": "madd-arid-lissukun",
				"start": 36,
				"end": 38
			}
		]
	},
	{
		"aya_id": 4996,
		"aya_number": 17,
		"aya_text": "يَطُوْفُ عَلَيْهِمْ وِلْدَانٌ مُّخَلَّدُوْنَۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "Mereka dikelilingi oleh anak-anak muda yang tetap muda,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 27,
				"end": 32
			},
			{
				"class": "madd-arid-lissukun",
				"start": 40,
				"end": 42
			}
		]
	},
	{
		"aya_id": 4997,
		"aya_number": 18,
		"aya_text": "بِاَكْوَابٍ وَّاَبَارِيْقَۙ وَكَأْسٍ مِّنْ مَّعِيْنٍۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "dengan membawa gelas, cerek dan sloki (piala) berisi minuman yang diambil dari air yang mengalir,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 9,
				"end": 14
			},
			{
				"class": "idgham-bighunnah",
				"start": 34,
				"end": 39
			},
			{
				"class": "idgham-bighunnah",
				"start": 40,
				"end": 45
			},
			{
				"class": "madd-arid-lissukun",
				"start": 48,
				"end": 50
			}
		]
	},
	{
		"aya_id": 4998,
		"aya_number": 19,
		"aya_text": "لَّا يُصَدَّعُوْنَ عَنْهَا وَلَا يُنْزِفُوْنَۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "mereka tidak pening karenanya dan tidak pula mabuk,",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 35,
				"end": 39
			},
			{
				"class": "madd-arid-lissukun",
				"start": 41,
				"end": 43
			}
		]
	},
	{
		"aya_id": 4999,
		"aya_number": 20,
		"aya_text": "وَفَاكِهَةٍ مِّمَّا يَتَخَيَّرُوْنَۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "dan buah-buahan apa pun yang mereka pilih,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 9,
				"end": 14
			},
			{
				"class": "ghunnah",
				"start": 15,
				"end": 17
			},
			{
				"class": "madd-arid-lissukun",
				"start": 31,
				"end": 33
			}
		]
	},
	{
		"aya_id": 5000,
		"aya_number": 21,
		"aya_text": "وَلَحْمِ طَيْرٍ مِّمَّا يَشْتَهُوْنَۗ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "dan daging burung apa pun yang mereka inginkan.",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 13,
				"end": 18
			},
			{
				"class": "ghunnah",
				"start": 19,
				"end": 21
			},
			{
				"class": "madd-arid-lissukun",
				"start": 32,
				"end": 34
			}
		]
	},
	{
		"aya_id": 5001,
		"aya_number": 22,
		"aya_text": "وَحُوْرٌ عِيْنٌۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "Dan ada bidadari-bidadari yang bermata indah,",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 11,
				"end": 13
			}
		]
	},
	{
		"aya_id": 5002,
		"aya_number": 23,
		"aya_text": "كَاَمْثَالِ اللُّؤْلُؤِ الْمَكْنُوْنِۚ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "laksana mutiara yang tersimpan baik.",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 33,
				"end": 35
			}
		]
	},
	{
		"aya_id": 5003,
		"aya_number": 24,
		"aya_text": "جَزَاۤءًۢ بِمَا كَانُوْا يَعْمَلُوْنَ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "Sebagai balasan atas apa yang mereka kerjakan.",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 4,
				"end": 6
			},
			{
				"class": "iqlab",
				"start": 6,
				"end": 8
			},
			{
				"class": "iqlab",
				"start": 9,
				"end": 11
			},
			{
				"class": "madd-arid-lissukun",
				"start": 33,
				"end": 35
			}
		]
	},
	{
		"aya_id": 5004,
		"aya_number": 25,
		"aya_text": "لَا يَسْمَعُوْنَ فِيْهَا لَغْوًا وَّلَا تَأْثِيْمًاۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "Di sana mereka tidak mendengar percakapan yang sia-sia maupun yang menimbulkan dosa,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 29,
				"end": 31
			},
			{
				"class": "idgham-bighunnah",
				"start": 32,
				"end": 34
			}
		]
	},
	{
		"aya_id": 5005,
		"aya_number": 26,
		"aya_text": "اِلَّا قِيْلًا سَلٰمًا سَلٰمًا ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "tetapi mereka mendengar ucapan salam.",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 11,
				"end": 13
			},
			{
				"class": "ikhfa",
				"start": 14,
				"end": 16
			},
			{
				"class": "ikhfa",
				"start": 19,
				"end": 21
			},
			{
				"class": "ikhfa",
				"start": 22,
				"end": 24
			}
		]
	},
	{
		"aya_id": 5006,
		"aya_number": 27,
		"aya_text": "وَاَصْحٰبُ الْيَمِينِ ەۙ مَآ اَصْحٰبُ الْيَمِيْنِۗ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "Dan golongan kanan, siapakah golongan kanan itu.",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 27,
				"end": 30
			},
			{
				"class": "madd-arid-lissukun",
				"start": 46,
				"end": 48
			}
		]
	},
	{
		"aya_id": 5007,
		"aya_number": 28,
		"aya_text": "فِيْ سِدْرٍ مَّخْضُوْدٍۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "(Mereka) berada di antara pohon bidara yang tidak berduri,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 7,
				"end": 8
			},
			{
				"class": "idgham-bighunnah",
				"start": 9,
				"end": 14
			},
			{
				"class": "madd-arid-lissukun",
				"start": 19,
				"end": 21
			}
		]
	},
	{
		"aya_id": 5008,
		"aya_number": 29,
		"aya_text": "وَّطَلْحٍ مَّنْضُوْدٍۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "dan pohon pisang yang bersusun-susun (buahnya),",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 7,
				"end": 12
			},
			{
				"class": "ikhfa",
				"start": 13,
				"end": 17
			},
			{
				"class": "madd-arid-lissukun",
				"start": 17,
				"end": 19
			}
		]
	},
	{
		"aya_id": 5009,
		"aya_number": 30,
		"aya_text": "وَّظِلٍّ مَّمْدُوْدٍۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "dan naungan yang terbentang luas,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 5,
				"end": 11
			},
			{
				"class": "madd-arid-lissukun",
				"start": 16,
				"end": 18
			}
		]
	},
	{
		"aya_id": 5010,
		"aya_number": 31,
		"aya_text": "وَّمَاۤءٍ مَّسْكُوْبٍۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "dan air yang mengalir terus-menerus,",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 5,
				"end": 7
			},
			{
				"class": "idgham-bighunnah",
				"start": 7,
				"end": 12
			},
			{
				"class": "madd-arid-lissukun",
				"start": 17,
				"end": 19
			}
		]
	},
	{
		"aya_id": 5011,
		"aya_number": 32,
		"aya_text": "وَّفَاكِهَةٍ كَثِيْرَةٍۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "dan buah-buahan yang banyak,",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 10,
				"end": 15
			},
			{
				"class": "madd-arid-lissukun",
				"start": 17,
				"end": 19
			}
		]
	},
	{
		"aya_id": 5012,
		"aya_number": 33,
		"aya_text": "لَّا مَقْطُوْعَةٍ وَّلَا مَمْنُوْعَةٍۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "yang tidak berhenti berbuah dan tidak terlarang mengambilnya,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 7,
				"end": 8
			},
			{
				"class": "idgham-bighunnah",
				"start": 15,
				"end": 20
			},
			{
				"class": "madd-arid-lissukun",
				"start": 31,
				"end": 33
			}
		]
	},
	{
		"aya_id": 5013,
		"aya_number": 34,
		"aya_text": "وَّفُرُشٍ مَّرْفُوْعَةٍۗ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "dan kasur-kasur yang tebal lagi empuk.",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 7,
				"end": 12
			},
			{
				"class": "madd-arid-lissukun",
				"start": 17,
				"end": 19
			}
		]
	},
	{
		"aya_id": 5014,
		"aya_number": 35,
		"aya_text": "اِنَّآ اَنْشَأْنٰهُنَّ اِنْشَاۤءًۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "Kami menciptakan mereka (bidadari-bidadari itu) secara langsung,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "madd-jaiz",
				"start": 5,
				"end": 8
			},
			{
				"class": "ikhfa",
				"start": 10,
				"end": 14
			},
			{
				"class": "ghunnah",
				"start": 20,
				"end": 22
			},
			{
				"class": "ikhfa",
				"start": 26,
				"end": 30
			},
			{
				"class": "madd-wajib",
				"start": 30,
				"end": 32
			}
		]
	},
	{
		"aya_id": 5015,
		"aya_number": 36,
		"aya_text": "فَجَعَلْنٰهُنَّ اَبْكَارًاۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "lalu Kami jadikan mereka perawan-perawan,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 12,
				"end": 14
			},
			{
				"class": "qalqalah",
				"start": 18,
				"end": 19
			}
		]
	},
	{
		"aya_id": 5016,
		"aya_number": 37,
		"aya_text": "عُرُبًا اَتْرَابًاۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "yang penuh cinta (dan) sebaya umurnya,",
		"tajweed": []
	},
	{
		"aya_id": 5017,
		"aya_number": 38,
		"aya_text": "لِّاَصْحٰبِ الْيَمِيْنِۗ  ࣖ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "untuk golongan kanan,",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 19,
				"end": 21
			}
		]
	},
	{
		"aya_id": 5018,
		"aya_number": 39,
		"aya_text": "ثُلَّةٌ مِّنَ الْاَوَّلِيْنَۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "segolongan besar dari orang-orang yang terdahulu,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 5,
				"end": 10
			},
			{
				"class": "madd-arid-lissukun",
				"start": 24,
				"end": 26
			}
		]
	},
	{
		"aya_id": 5019,
		"aya_number": 40,
		"aya_text": "وَثُلَّةٌ مِّنَ الْاٰخِرِيْنَۗ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "dan segolongan besar pula dari orang yang kemudian.",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 7,
				"end": 12
			},
			{
				"class": "madd-arid-lissukun",
				"start": 25,
				"end": 27
			}
		]
	},
	{
		"aya_id": 5020,
		"aya_number": 41,
		"aya_text": "وَاَصْحٰبُ الشِّمَالِ ەۙ مَآ اَصْحٰبُ الشِّمَالِۗ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "Dan golongan kiri, alangkah sengsaranya golongan kiri itu.",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 27,
				"end": 30
			},
			{
				"class": "madd-arid-lissukun",
				"start": 46,
				"end": 47
			}
		]
	},
	{
		"aya_id": 5021,
		"aya_number": 42,
		"aya_text": "فِيْ سَمُوْمٍ وَّحَمِيْمٍۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "(Mereka) dalam siksaan angin yang sangat panas dan air yang mendidih,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 11,
				"end": 16
			},
			{
				"class": "madd-arid-lissukun",
				"start": 21,
				"end": 23
			}
		]
	},
	{
		"aya_id": 5022,
		"aya_number": 43,
		"aya_text": "وَّظِلٍّ مِّنْ يَّحْمُوْمٍۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "dan naungan asap yang hitam,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 5,
				"end": 11
			},
			{
				"class": "idgham-bighunnah",
				"start": 12,
				"end": 17
			},
			{
				"class": "madd-arid-lissukun",
				"start": 22,
				"end": 24
			}
		]
	},
	{
		"aya_id": 5023,
		"aya_number": 44,
		"aya_text": "لَّا بَارِدٍ وَّلَا كَرِيْمٍ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "tidak sejuk dan tidak menyenangkan.",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 10,
				"end": 15
			},
			{
				"class": "madd-arid-lissukun",
				"start": 24,
				"end": 26
			}
		]
	},
	{
		"aya_id": 5024,
		"aya_number": 45,
		"aya_text": "اِنَّهُمْ كَانُوْا قَبْلَ ذٰلِكَ مُتْرَفِيْنَۚ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "Sesungguhnya mereka sebelum itu (dahulu) hidup bermewah-mewah,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "qalqalah",
				"start": 21,
				"end": 22
			},
			{
				"class": "madd-arid-lissukun",
				"start": 41,
				"end": 43
			}
		]
	},
	{
		"aya_id": 5025,
		"aya_number": 46,
		"aya_text": "وَكَانُوْا يُصِرُّوْنَ عَلَى الْحِنْثِ الْعَظِيْمِۚ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "dan mereka terus-menerus mengerjakan dosa yang besar,",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 34,
				"end": 38
			},
			{
				"class": "madd-arid-lissukun",
				"start": 46,
				"end": 48
			}
		]
	},
	{
		"aya_id": 5026,
		"aya_number": 47,
		"aya_text": "وَكَانُوْا يَقُوْلُوْنَ ەۙ اَىِٕذَا مِتْنَا وَكُنَّا تُرَابًا وَّعِظَامًا ءَاِنَّا لَمَبْعُوْثُوْنَۙ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "dan mereka berkata, “Apabila kami sudah mati, menjadi tanah dan tulang-belulang, apakah kami benar-benar akan dibangkitkan kembali?",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 48,
				"end": 50
			},
			{
				"class": "idgham-bighunnah",
				"start": 58,
				"end": 60
			},
			{
				"class": "idgham-bighunnah",
				"start": 61,
				"end": 63
			},
			{
				"class": "ghunnah",
				"start": 78,
				"end": 80
			},
			{
				"class": "qalqalah",
				"start": 87,
				"end": 88
			},
			{
				"class": "madd-arid-lissukun",
				"start": 95,
				"end": 97
			}
		]
	},
	{
		"aya_id": 5027,
		"aya_number": 48,
		"aya_text": "اَوَاٰبَاۤؤُنَا الْاَوَّلُوْنَ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "Apakah nenek moyang kami yang terdahulu (dibangkitkan pula)?”",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 26,
				"end": 28
			}
		]
	},
	{
		"aya_id": 5028,
		"aya_number": 49,
		"aya_text": "قُلْ اِنَّ الْاَوَّلِيْنَ وَالْاٰخِرِيْنَۙ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "Katakanlah, “(Ya), sesungguhnya orang-orang yang terdahulu dan yang kemudian,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 7,
				"end": 9
			},
			{
				"class": "madd-arid-lissukun",
				"start": 37,
				"end": 39
			}
		]
	},
	{
		"aya_id": 5029,
		"aya_number": 50,
		"aya_text": "لَمَجْمُوْعُوْنَۙ اِلٰى مِيْقَاتِ يَوْمٍ مَّعْلُوْمٍ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 535,
		"translation_aya_text": "pasti semua akan dikumpulkan pada waktu tertentu, pada hari yang sudah dimaklumi.",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 4,
				"end": 5
			},
			{
				"class": "idgham-bighunnah",
				"start": 38,
				"end": 43
			},
			{
				"class": "madd-arid-lissukun",
				"start": 48,
				"end": 50
			}
		]
	},
	{
		"aya_id": 5030,
		"aya_number": 51,
		"aya_text": "ثُمَّ اِنَّكُمْ اَيُّهَا الضَّاۤ لُّوْنَ الْمُكَذِّبُوْنَۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Kemudian sesungguhnya kamu, wahai orang-orang yang sesat lagi mendustakan!",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "ghunnah",
				"start": 8,
				"end": 10
			},
			{
				"class": "madd-arid-lissukun",
				"start": 53,
				"end": 55
			}
		]
	},
	{
		"aya_id": 5031,
		"aya_number": 52,
		"aya_text": "لَاٰكِلُوْنَ مِنْ شَجَرٍ مِّنْ زَقُّوْمٍۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "pasti akan memakan pohon zaqqum,",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 15,
				"end": 20
			},
			{
				"class": "idgham-bighunnah",
				"start": 22,
				"end": 27
			},
			{
				"class": "ikhfa",
				"start": 28,
				"end": 33
			},
			{
				"class": "madd-arid-lissukun",
				"start": 36,
				"end": 38
			}
		]
	},
	{
		"aya_id": 5032,
		"aya_number": 53,
		"aya_text": "فَمَالِـُٔوْنَ مِنْهَا الْبُطُوْنَۚ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "maka akan penuh perutmu dengannya.",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 30,
				"end": 32
			}
		]
	},
	{
		"aya_id": 5033,
		"aya_number": 54,
		"aya_text": "فَشَارِبُوْنَ عَلَيْهِ مِنَ الْحَمِيْمِۚ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Setelah itu kamu akan meminum air yang sangat panas.",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 35,
				"end": 37
			}
		]
	},
	{
		"aya_id": 5034,
		"aya_number": 55,
		"aya_text": "فَشَارِبُوْنَ شُرْبَ الْهِيْمِۗ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Maka kamu minum seperti unta (yang sangat haus) minum.",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 26,
				"end": 28
			}
		]
	},
	{
		"aya_id": 5035,
		"aya_number": 56,
		"aya_text": "هٰذَا نُزُلُهُمْ يَوْمَ الدِّيْنِۗ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Itulah hidangan untuk mereka pada hari pembalasan.”",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 29,
				"end": 31
			}
		]
	},
	{
		"aya_id": 5036,
		"aya_number": 57,
		"aya_text": "نَحْنُ خَلَقْنٰكُمْ فَلَوْلَا تُصَدِّقُوْنَ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Kami telah menciptakan kamu, mengapa kamu tidak membenarkan (hari berbangkit)?",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 11,
				"end": 12
			},
			{
				"class": "madd-arid-lissukun",
				"start": 39,
				"end": 41
			}
		]
	},
	{
		"aya_id": 5037,
		"aya_number": 58,
		"aya_text": "اَفَرَءَيْتُمْ مَّا تُمْنُوْنَۗ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Maka adakah kamu perhatikan, tentang (benih manusia) yang kamu pancarkan.",
		"tajweed": [
			{
				"class": "idgham-mimi",
				"start": 12,
				"end": 17
			},
			{
				"class": "madd-arid-lissukun",
				"start": 26,
				"end": 28
			}
		]
	},
	{
		"aya_id": 5038,
		"aya_number": 59,
		"aya_text": "ءَاَنْتُمْ تَخْلُقُوْنَهٗٓ اَمْ نَحْنُ الْخَالِقُوْنَ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Kamukah yang menciptakannya, ataukah Kami penciptanya?",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 4,
				"end": 8
			},
			{
				"class": "madd-jaiz",
				"start": 23,
				"end": 27
			},
			{
				"class": "madd-arid-lissukun",
				"start": 49,
				"end": 51
			}
		]
	},
	{
		"aya_id": 5039,
		"aya_number": 60,
		"aya_text": "نَحْنُ قَدَّرْنَا بَيْنَكُمُ الْمَوْتَ وَمَا نَحْنُ بِمَسْبُوْقِيْنَۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Kami telah menentukan kematian masing-masing kamu dan Kami tidak lemah,",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 64,
				"end": 66
			}
		]
	},
	{
		"aya_id": 5040,
		"aya_number": 61,
		"aya_text": "عَلٰٓى اَنْ نُّبَدِّلَ اَمْثَالَكُمْ وَنُنْشِئَكُمْ فِيْ مَا لَا تَعْلَمُوْنَ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "untuk menggantikan kamu dengan orang-orang yang seperti kamu (di dunia) dan membangkitkan kamu kelak (di akhirat) dalam keadaan yang tidak kamu ketahui.",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 2,
				"end": 7
			},
			{
				"class": "idgham-bighunnah",
				"start": 9,
				"end": 14
			},
			{
				"class": "ikhfa",
				"start": 41,
				"end": 45
			},
			{
				"class": "madd-arid-lissukun",
				"start": 73,
				"end": 75
			}
		]
	},
	{
		"aya_id": 5041,
		"aya_number": 62,
		"aya_text": "وَلَقَدْ عَلِمْتُمُ النَّشْاَةَ الْاُوْلٰى فَلَوْلَا تَذَكَّرُوْنَ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Dan sungguh, kamu telah tahu penciptaan yang pertama, mengapa kamu tidak mengambil pelajaran?",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 6,
				"end": 7
			},
			{
				"class": "ghunnah",
				"start": 22,
				"end": 24
			},
			{
				"class": "madd-arid-lissukun",
				"start": 62,
				"end": 64
			}
		]
	},
	{
		"aya_id": 5042,
		"aya_number": 63,
		"aya_text": "اَفَرَءَيْتُمْ مَّا تَحْرُثُوْنَۗ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Pernahkah kamu perhatikan benih yang kamu tanam?",
		"tajweed": [
			{
				"class": "idgham-mimi",
				"start": 12,
				"end": 17
			},
			{
				"class": "madd-arid-lissukun",
				"start": 28,
				"end": 30
			}
		]
	},
	{
		"aya_id": 5043,
		"aya_number": 64,
		"aya_text": "ءَاَنْتُمْ تَزْرَعُوْنَهٗٓ اَمْ نَحْنُ الزَّارِعُوْنَ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Kamukah yang menumbuhkannya ataukah Kami yang menumbuhkan?",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 4,
				"end": 8
			},
			{
				"class": "madd-jaiz",
				"start": 23,
				"end": 27
			},
			{
				"class": "madd-arid-lissukun",
				"start": 49,
				"end": 51
			}
		]
	},
	{
		"aya_id": 5044,
		"aya_number": 65,
		"aya_text": "لَوْ نَشَاۤءُ لَجَعَلْنٰهُ حُطَامًا فَظَلْتُمْ تَفَكَّهُوْنَۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Sekiranya Kami kehendaki, niscaya Kami hancurkan sampai lumat; maka kamu akan heran tercengang,",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 9,
				"end": 11
			},
			{
				"class": "ikhfa",
				"start": 32,
				"end": 34
			},
			{
				"class": "ikhfa",
				"start": 35,
				"end": 37
			},
			{
				"class": "madd-arid-lissukun",
				"start": 56,
				"end": 58
			}
		]
	},
	{
		"aya_id": 5045,
		"aya_number": 66,
		"aya_text": "اِنَّا لَمُغْرَمُوْنَۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "(sambil berkata), “Sungguh, kami benar-benar menderita kerugian,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "madd-arid-lissukun",
				"start": 17,
				"end": 19
			}
		]
	},
	{
		"aya_id": 5046,
		"aya_number": 67,
		"aya_text": "بَلْ نَحْنُ مَحْرُوْمُوْنَ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "bahkan kami tidak mendapat hasil apa pun.”",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 22,
				"end": 24
			}
		]
	},
	{
		"aya_id": 5047,
		"aya_number": 68,
		"aya_text": "اَفَرَءَيْتُمُ الْمَاۤءَ الَّذِيْ تَشْرَبُوْنَۗ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Pernahkah kamu memperhatikan air yang kamu minum?",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 20,
				"end": 22
			},
			{
				"class": "madd-arid-lissukun",
				"start": 42,
				"end": 44
			}
		]
	},
	{
		"aya_id": 5048,
		"aya_number": 69,
		"aya_text": "ءَاَنْتُمْ اَنْزَلْتُمُوْهُ مِنَ الْمُزْنِ اَمْ نَحْنُ الْمُنْزِلُوْنَ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Kamukah yang menurunkannya dari awan ataukah Kami yang menurunkan?",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 4,
				"end": 8
			},
			{
				"class": "ikhfa",
				"start": 13,
				"end": 17
			},
			{
				"class": "ikhfa",
				"start": 60,
				"end": 64
			},
			{
				"class": "madd-arid-lissukun",
				"start": 66,
				"end": 68
			}
		]
	},
	{
		"aya_id": 5049,
		"aya_number": 70,
		"aya_text": "لَوْ نَشَاۤءُ جَعَلْنٰهُ اُجَاجًا فَلَوْلَا تَشْكُرُوْنَ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Sekiranya Kami menghendaki, niscaya Kami menjadikannya asin, mengapa kamu tidak bersyukur?",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 9,
				"end": 11
			},
			{
				"class": "ikhfa",
				"start": 30,
				"end": 32
			},
			{
				"class": "ikhfa",
				"start": 33,
				"end": 35
			},
			{
				"class": "madd-arid-lissukun",
				"start": 52,
				"end": 54
			}
		]
	},
	{
		"aya_id": 5050,
		"aya_number": 71,
		"aya_text": "اَفَرَءَيْتُمُ النَّارَ الَّتِيْ تُوْرُوْنَۗ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Maka pernahkah kamu memperhatikan tentang api yang kamu nyalakan (dengan kayu)?",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 17,
				"end": 19
			},
			{
				"class": "madd-arid-lissukun",
				"start": 39,
				"end": 41
			}
		]
	},
	{
		"aya_id": 5051,
		"aya_number": 72,
		"aya_text": "ءَاَنْتُمْ اَنْشَأْتُمْ شَجَرَتَهَآ اَمْ نَحْنُ الْمُنْشِـُٔوْنَ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Kamukah yang menumbuhkan kayu itu ataukah Kami yang menumbuhkan?",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 4,
				"end": 8
			},
			{
				"class": "ikhfa",
				"start": 13,
				"end": 17
			},
			{
				"class": "madd-jaiz",
				"start": 34,
				"end": 37
			},
			{
				"class": "ikhfa",
				"start": 54,
				"end": 58
			},
			{
				"class": "madd-arid-lissukun",
				"start": 61,
				"end": 63
			}
		]
	},
	{
		"aya_id": 5052,
		"aya_number": 73,
		"aya_text": "نَحْنُ جَعَلْنٰهَا تَذْكِرَةً وَّمَتَاعًا لِّلْمُقْوِيْنَۚ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Kami menjadikannya (api itu) untuk peringatan dan bahan yang berguna bagi musafir.",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 27,
				"end": 32
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 38,
				"end": 40
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 41,
				"end": 43
			},
			{
				"class": "qalqalah",
				"start": 49,
				"end": 50
			},
			{
				"class": "madd-arid-lissukun",
				"start": 53,
				"end": 55
			}
		]
	},
	{
		"aya_id": 5053,
		"aya_number": 74,
		"aya_text": "فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِيْمِ ࣖ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Maka bertasbihlah dengan (menyebut) nama Tuhanmu Yang Mahabesar.",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 33,
				"end": 35
			}
		]
	},
	{
		"aya_id": 5054,
		"aya_number": 75,
		"aya_text": "فَلَآ اُقْسِمُ بِمَوٰقِعِ النُّجُوْمِ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Lalu Aku bersumpah dengan tempat beredarnya bintang-bintang.",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 4,
				"end": 7
			},
			{
				"class": "qalqalah",
				"start": 9,
				"end": 10
			},
			{
				"class": "ghunnah",
				"start": 29,
				"end": 31
			},
			{
				"class": "madd-arid-lissukun",
				"start": 34,
				"end": 36
			}
		]
	},
	{
		"aya_id": 5055,
		"aya_number": 76,
		"aya_text": "وَاِنَّهٗ لَقَسَمٌ لَّوْ تَعْلَمُوْنَ عَظِيْمٌۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 536,
		"translation_aya_text": "Dan sesungguhnya itu benar-benar sumpah yang besar sekiranya kamu mengetahui,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 6
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 16,
				"end": 21
			},
			{
				"class": "madd-arid-lissukun",
				"start": 42,
				"end": 44
			}
		]
	},
	{
		"aya_id": 5056,
		"aya_number": 77,
		"aya_text": "اِنَّهٗ لَقُرْاٰنٌ كَرِيْمٌۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 537,
		"translation_aya_text": "dan (ini) sesungguhnya Al-Qur'an yang sangat mulia,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "ikhfa",
				"start": 16,
				"end": 21
			},
			{
				"class": "madd-arid-lissukun",
				"start": 23,
				"end": 25
			}
		]
	},
	{
		"aya_id": 5057,
		"aya_number": 78,
		"aya_text": "فِيْ كِتٰبٍ مَّكْنُوْنٍۙ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 537,
		"translation_aya_text": "dalam Kitab yang terpelihara (Lauh Mahfuzh),",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 9,
				"end": 14
			},
			{
				"class": "madd-arid-lissukun",
				"start": 19,
				"end": 21
			}
		]
	},
	{
		"aya_id": 5058,
		"aya_number": 79,
		"aya_text": "لَّا يَمَسُّهٗٓ اِلَّا الْمُطَهَّرُوْنَۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 537,
		"translation_aya_text": "tidak ada yang menyentuhnya selain hamba-hamba yang disucikan.",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 12,
				"end": 16
			},
			{
				"class": "madd-arid-lissukun",
				"start": 35,
				"end": 37
			}
		]
	},
	{
		"aya_id": 5059,
		"aya_number": 80,
		"aya_text": "تَنْزِيْلٌ مِّنْ رَّبِّ الْعٰلَمِيْنَ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 537,
		"translation_aya_text": "Diturunkan dari Tuhan seluruh alam.",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 2,
				"end": 6
			},
			{
				"class": "idgham-bighunnah",
				"start": 8,
				"end": 13
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 14,
				"end": 19
			},
			{
				"class": "madd-arid-lissukun",
				"start": 33,
				"end": 35
			}
		]
	},
	{
		"aya_id": 5060,
		"aya_number": 81,
		"aya_text": "اَفَبِهٰذَا الْحَدِيْثِ اَنْتُمْ مُّدْهِنُوْنَ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 537,
		"translation_aya_text": "Apakah kamu menganggap remeh berita ini (Al-Qur'an),",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 26,
				"end": 30
			},
			{
				"class": "idgham-mimi",
				"start": 30,
				"end": 35
			},
			{
				"class": "qalqalah",
				"start": 36,
				"end": 37
			},
			{
				"class": "madd-arid-lissukun",
				"start": 42,
				"end": 44
			}
		]
	},
	{
		"aya_id": 5061,
		"aya_number": 82,
		"aya_text": "وَتَجْعَلُوْنَ رِزْقَكُمْ اَنَّكُمْ تُكَذِّبُوْنَ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 537,
		"translation_aya_text": "dan kamu menjadikan rezeki yang kamu terima (dari Allah) justru untuk mendustakan(-Nya).",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 4,
				"end": 5
			},
			{
				"class": "ghunnah",
				"start": 28,
				"end": 30
			},
			{
				"class": "madd-arid-lissukun",
				"start": 45,
				"end": 47
			}
		]
	},
	{
		"aya_id": 5062,
		"aya_number": 83,
		"aya_text": "فَلَوْلَآ اِذَا بَلَغَتِ الْحُلْقُوْمَۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 537,
		"translation_aya_text": "Maka kalau begitu mengapa (tidak mencegah) ketika (nyawa) telah sampai di kerongkongan,",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 8,
				"end": 11
			},
			{
				"class": "madd-arid-lissukun",
				"start": 35,
				"end": 37
			}
		]
	},
	{
		"aya_id": 5063,
		"aya_number": 84,
		"aya_text": "وَاَنْتُمْ حِيْنَىِٕذٍ تَنْظُرُوْنَۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 537,
		"translation_aya_text": "dan kamu ketika itu melihat,",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 4,
				"end": 8
			},
			{
				"class": "ikhfa",
				"start": 20,
				"end": 25
			},
			{
				"class": "ikhfa",
				"start": 25,
				"end": 29
			},
			{
				"class": "madd-arid-lissukun",
				"start": 31,
				"end": 33
			}
		]
	},
	{
		"aya_id": 5064,
		"aya_number": 85,
		"aya_text": "وَنَحْنُ اَقْرَبُ اِلَيْهِ مِنْكُمْ وَلٰكِنْ لَّا تُبْصِرُوْنَ ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 537,
		"translation_aya_text": "dan Kami lebih dekat kepadanya daripada kamu, tetapi kamu tidak melihat,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 11,
				"end": 12
			},
			{
				"class": "ikhfa",
				"start": 29,
				"end": 33
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 42,
				"end": 47
			},
			{
				"class": "qalqalah",
				"start": 52,
				"end": 53
			},
			{
				"class": "madd-arid-lissukun",
				"start": 58,
				"end": 60
			}
		]
	},
	{
		"aya_id": 5065,
		"aya_number": 86,
		"aya_text": "فَلَوْلَآ اِنْ كُنْتُمْ غَيْرَ مَدِيْنِيْنَۙ  ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 537,
		"translation_aya_text": "maka mengapa jika kamu memang tidak dikuasai (oleh Allah),",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 8,
				"end": 11
			},
			{
				"class": "ikhfa",
				"start": 13,
				"end": 18
			},
			{
				"class": "ikhfa",
				"start": 18,
				"end": 22
			},
			{
				"class": "madd-arid-lissukun",
				"start": 40,
				"end": 42
			}
		]
	},
	{
		"aya_id": 5066,
		"aya_number": 87,
		"aya_text": "تَرْجِعُوْنَهَآ اِنْ كُنْتُمْ صٰدِقِيْنَ",
		"sura_id": 56,
		"juz_id": 27,
		"page_number": 537,
		"translation_aya_text": "kamu tidak mengembalikannya (nyawa itu) jika kamu orang yang benar?",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 14,
				"end": 17
			},
			{
				"class": "ikhfa",
				"start": 19,
				"end": 24
			},
			{
