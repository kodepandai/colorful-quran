[
	{
		"aya_id": 5885,
		"aya_number": 1,
		"aya_text": "اِذَا السَّمَاۤءُ انْشَقَّتْۙ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "Apabila langit terbelah,",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 13,
				"end": 15
			},
			{
				"class": "ikhfa",
				"start": 19,
				"end": 23
			}
		]
	},
	{
		"aya_id": 5886,
		"aya_number": 2,
		"aya_text": "وَاَذِنَتْ لِرَبِّهَا وَحُقَّتْۙ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "dan patuh kepada Tuhannya, dan sudah semestinya patuh, ",
		"tajweed": []
	},
	{
		"aya_id": 5887,
		"aya_number": 3,
		"aya_text": "وَاِذَا الْاَرْضُ مُدَّتْۙ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "dan apabila bumi diratakan, ",
		"tajweed": []
	},
	{
		"aya_id": 5888,
		"aya_number": 4,
		"aya_text": "وَاَلْقَتْ مَا فِيْهَا وَتَخَلَّتْۙ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "dan memuntahkan apa yang ada di dalamnya dan menjadi kosong,",
		"tajweed": []
	},
	{
		"aya_id": 5889,
		"aya_number": 5,
		"aya_text": "وَاَذِنَتْ لِرَبِّهَا وَحُقَّتْۗ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "dan patuh kepada Tuhannya, dan sudah semestinya patuh.",
		"tajweed": []
	},
	{
		"aya_id": 5890,
		"aya_number": 6,
		"aya_text": "يٰٓاَيُّهَا الْاِنْسَانُ اِنَّكَ كَادِحٌ اِلٰى رَبِّكَ كَدْحًا فَمُلٰقِيْهِۚ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "Wahai manusia! Sesungguhnya kamu telah bekerja keras menuju Tuhanmu, maka kamu akan menemui-Nya.",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 0,
				"end": 3
			},
			{
				"class": "ikhfa",
				"start": 17,
				"end": 21
			},
			{
				"class": "ghunnah",
				"start": 27,
				"end": 30
			},
			{
				"class": "qalqalah",
				"start": 57,
				"end": 58
			},
			{
				"class": "ikhfa",
				"start": 59,
				"end": 61
			},
			{
				"class": "ikhfa",
				"start": 62,
				"end": 64
			},
			{
				"class": "madd-arid-lissukun",
				"start": 71,
				"end": 73
			}
		]
	},
	{
		"aya_id": 5891,
		"aya_number": 7,
		"aya_text": "فَاَمَّا مَنْ اُوْتِيَ كِتٰبَهٗ بِيَمِيْنِهٖۙ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "Maka adapun orang yang catatannya diberikan dari sebelah kanannya, ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			}
		]
	},
	{
		"aya_id": 5892,
		"aya_number": 8,
		"aya_text": "فَسَوْفَ يُحَاسَبُ حِسَابًا يَّسِيْرًاۙ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "maka dia akan diperiksa dengan pemeriksaan yang mudah, ",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 24,
				"end": 26
			},
			{
				"class": "idgham-bighunnah",
				"start": 27,
				"end": 29
			}
		]
	},
	{
		"aya_id": 5893,
		"aya_number": 9,
		"aya_text": "وَّيَنْقَلِبُ اِلٰٓى اَهْلِهٖ مَسْرُوْرًاۗ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "dan dia akan kembali kepada keluarganya (yang sama-sama beriman) dengan gembira.",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 5,
				"end": 9
			},
			{
				"class": "madd-jaiz",
				"start": 16,
				"end": 21
			}
		]
	},
	{
		"aya_id": 5894,
		"aya_number": 10,
		"aya_text": "وَاَمَّا مَنْ اُوْتِيَ كِتٰبَهٗ وَرَاۤءَ ظَهْرِهٖۙ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "Dan adapun orang yang catatannya diberikan dari sebelah belakang, ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			},
			{
				"class": "madd-wajib",
				"start": 36,
				"end": 38
			}
		]
	},
	{
		"aya_id": 5895,
		"aya_number": 11,
		"aya_text": "فَسَوْفَ يَدْعُوْ ثُبُوْرًاۙ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "maka dia akan berteriak, “Celakalah aku!” ",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 11,
				"end": 12
			}
		]
	},
	{
		"aya_id": 5896,
		"aya_number": 12,
		"aya_text": "وَّيَصْلٰى سَعِيْرًاۗ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "Dan dia akan masuk ke dalam api yang menyala-nyala (neraka).",
		"tajweed": []
	},
	{
		"aya_id": 5897,
		"aya_number": 13,
		"aya_text": "اِنَّهٗ كَانَ فِيْٓ اَهْلِهٖ مَسْرُوْرًاۗ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "Sungguh, dia dahulu (di dunia) bergembira di kalangan keluarganya (yang sama-sama kafir). ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			},
			{
				"class": "madd-jaiz",
				"start": 16,
				"end": 20
			}
		]
	},
	{
		"aya_id": 5898,
		"aya_number": 14,
		"aya_text": "اِنَّهٗ ظَنَّ اَنْ لَّنْ يَّحُوْرَ ۛ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "Sesungguhnya dia mengira bahwa dia tidak akan kembali (kepada Tuhannya). ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			},
			{
				"class": "ghunnah",
				"start": 10,
				"end": 13
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 16,
				"end": 21
			},
			{
				"class": "idgham-bighunnah",
				"start": 22,
				"end": 27
			},
			{
				"class": "madd-arid-lissukun",
				"start": 30,
				"end": 32
			}
		]
	},
	{
		"aya_id": 5899,
		"aya_number": 15,
		"aya_text": "بَلٰىۛ اِنَّ رَبَّهٗ كَانَ بِهٖ بَصِيْرًاۗ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "Tidak demikian, sesungguhnya Tuhannya selalu melihatnya. ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 9,
				"end": 12
			}
		]
	},
	{
		"aya_id": 5900,
		"aya_number": 16,
		"aya_text": "فَلَآ اُقْسِمُ بِالشَّفَقِۙ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "Maka Aku bersumpah demi cahaya merah pada waktu senja, ",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 4,
				"end": 7
			},
			{
				"class": "qalqalah",
				"start": 9,
				"end": 10
			}
		]
	},
	{
		"aya_id": 5901,
		"aya_number": 17,
		"aya_text": "وَالَّيْلِ وَمَا وَسَقَۙ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "demi malam dan apa yang diselubunginya,",
		"tajweed": []
	},
	{
		"aya_id": 5902,
		"aya_number": 18,
		"aya_text": "وَالْقَمَرِ اِذَا اتَّسَقَۙ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "demi bulan apabila jadi purnama,",
		"tajweed": []
	},
	{
		"aya_id": 5903,
		"aya_number": 19,
		"aya_text": "لَتَرْكَبُنَّ طَبَقًا عَنْ طَبَقٍۗ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "sungguh, akan kamu jalani tingkat demi tingkat (dalam kehidupan).",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 10,
				"end": 13
			},
			{
				"class": "ikhfa",
				"start": 24,
				"end": 29
			}
		]
	},
	{
		"aya_id": 5904,
		"aya_number": 20,
		"aya_text": "فَمَا لَهُمْ لَا يُؤْمِنُوْنَۙ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "Maka mengapa mereka tidak mau beriman?",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 25,
				"end": 27
			}
		]
	},
	{
		"aya_id": 5905,
		"aya_number": 21,
		"aya_text": "وَاِذَا قُرِئَ عَلَيْهِمُ الْقُرْاٰنُ لَا يَسْجُدُوْنَ ۗ ۩ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "Dan apabila Al-Qur'an dibacakan kepada mereka, mereka tidak (mau) bersujud,",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 50,
				"end": 52
			}
		]
	},
	{
		"aya_id": 5906,
		"aya_number": 22,
		"aya_text": "بَلِ الَّذِيْنَ كَفَرُوْا يُكَذِّبُوْنَۖ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "bahkan orang-orang kafir itu mendustakan(nya).",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 35,
				"end": 37
			}
		]
	},
	{
		"aya_id": 5907,
		"aya_number": 23,
		"aya_text": "وَاللّٰهُ اَعْلَمُ بِمَا يُوْعُوْنَۖ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "Dan Allah lebih mengetahui apa yang mereka sembunyikan (dalam hati mereka).",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 31,
				"end": 33
			}
		]
	},
	{
		"aya_id": 5908,
		"aya_number": 24,
		"aya_text": "فَبَشِّرْهُمْ بِعَذَابٍ اَلِيْمٍۙ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "Maka sampaikanlah kepada mereka (ancaman) azab yang pedih,",
		"tajweed": [
			{
				"class": "ikhfa-syafawi",
				"start": 11,
				"end": 16
			},
			{
				"class": "madd-arid-lissukun",
				"start": 28,
				"end": 30
			}
		]
	},
	{
		"aya_id": 5909,
		"aya_number": 25,
		"aya_text": "اِلَّا الَّذِيْنَ اٰمَنُوْا وَعَمِلُوا الصّٰلِحٰتِ لَهُمْ اَجْرٌ غَيْرُ مَمْنُوْنٍ ࣖ ",
		"sura_id": 84,
		"juz_id": 30,
		"page_number": 589,
		"translation_aya_text": "kecuali orang-orang yang beriman dan mengerjakan kebajikan, mereka akan mendapat pahala yang tidak putus-putusnya.",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 60,
				"end": 61
			},
			{
				"class": "madd-arid-lissukun",
				"start": 78,
				"end": 80
			}
		]
	}
]
