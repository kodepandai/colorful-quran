[
	{
		"aya_id": 5376,
		"aya_number": 1,
		"aya_text": "سَاَلَ سَاۤىِٕلٌۢ بِعَذَابٍ وَّاقِعٍۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Seseorang bertanya tentang azab yang pasti terjadi, ",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 8,
				"end": 12
			},
			{
				"class": "iqlab",
				"start": 14,
				"end": 16
			},
			{
				"class": "iqlab",
				"start": 17,
				"end": 19
			},
			{
				"class": "idgham-bighunnah",
				"start": 25,
				"end": 30
			}
		]
	},
	{
		"aya_id": 5377,
		"aya_number": 2,
		"aya_text": "لِّلْكٰفِرِيْنَ لَيْسَ لَهٗ دَافِعٌۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "bagi orang-orang kafir, yang tidak seorang pun dapat menolaknya,",
		"tajweed": []
	},
	{
		"aya_id": 5378,
		"aya_number": 3,
		"aya_text": "مِّنَ اللّٰهِ ذِى الْمَعَارِجِۗ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "(Azab) dari Allah, yang memiliki tempat-tempat naik.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 0,
				"end": 4
			}
		]
	},
	{
		"aya_id": 5379,
		"aya_number": 4,
		"aya_text": "تَعْرُجُ الْمَلٰۤىِٕكَةُ وَالرُّوْحُ اِلَيْهِ فِيْ يَوْمٍ كَانَ مِقْدَارُهٗ خَمْسِيْنَ اَلْفَ سَنَةٍۚ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Para malaikat dan Jibril naik (menghadap) kepada Tuhan, dalam sehari setara dengan lima puluh ribu tahun.",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 14,
				"end": 18
			},
			{
				"class": "ikhfa",
				"start": 55,
				"end": 60
			},
			{
				"class": "qalqalah",
				"start": 66,
				"end": 67
			}
		]
	},
	{
		"aya_id": 5380,
		"aya_number": 5,
		"aya_text": "فَاصْبِرْ صَبْرًا جَمِيْلًا",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Maka bersabarlah engkau (Muhammad) dengan kesabaran yang baik.",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 12,
				"end": 13
			},
			{
				"class": "ikhfa",
				"start": 14,
				"end": 16
			},
			{
				"class": "ikhfa",
				"start": 17,
				"end": 19
			},
			{
				"class": "madd-arid-lissukun",
				"start": 22,
				"end": 24
			}
		]
	},
	{
		"aya_id": 5381,
		"aya_number": 6,
		"aya_text": "اِنَّهُمْ يَرَوْنَهٗ بَعِيْدًاۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Mereka memandang (azab) itu jauh (mustahil). ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			}
		]
	},
	{
		"aya_id": 5382,
		"aya_number": 7,
		"aya_text": "وَّنَرٰىهُ قَرِيْبًاۗ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Sedangkan Kami memandangnya dekat (pasti terjadi). ",
		"tajweed": []
	},
	{
		"aya_id": 5383,
		"aya_number": 8,
		"aya_text": "يَوْمَ تَكُوْنُ السَّمَاۤءُ كَالْمُهْلِۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "(Ingatlah) pada hari ketika langit men-jadi bagaikan cairan tembaga, ",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 22,
				"end": 25
			}
		]
	},
	{
		"aya_id": 5384,
		"aya_number": 9,
		"aya_text": "وَتَكُوْنُ الْجِبَالُ كَالْعِهْنِۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "dan gunung-gunung bagaikan bulu (yang beterbangan), ",
		"tajweed": []
	},
	{
		"aya_id": 5385,
		"aya_number": 10,
		"aya_text": "وَلَا يَسْـَٔلُ حَمِيْمٌ حَمِيْمًاۚ ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "dan tidak ada seorang teman karib pun menanyakan temannya,",
		"tajweed": []
	},
	{
		"aya_id": 5386,
		"aya_number": 11,
		"aya_text": "يُبَصَّرُوْنَهُمْۗ يَوَدُّ الْمُجْرِمُ لَوْ يَفْتَدِيْ مِنْ عَذَابِ يَوْمِىِٕذٍۢ بِبَنِيْهِۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "sedang mereka saling melihat. Pada hari itu, orang yang berdosa ingin sekiranya dia dapat menebus (dirinya) dari azab dengan anak-anaknya, ",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 32,
				"end": 33
			},
			{
				"class": "iqlab",
				"start": 77,
				"end": 79
			},
			{
				"class": "iqlab",
				"start": 80,
				"end": 82
			},
			{
				"class": "madd-arid-lissukun",
				"start": 87,
				"end": 89
			}
		]
	},
	{
		"aya_id": 5387,
		"aya_number": 12,
		"aya_text": "وَصَاحِبَتِهٖ وَاَخِيْهِۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "dan istrinya dan saudaranya, ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 20,
				"end": 22
			}
		]
	},
	{
		"aya_id": 5388,
		"aya_number": 13,
		"aya_text": "وَفَصِيْلَتِهِ الَّتِيْ تُـْٔوِيْهِۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "dan keluarga yang melindunginya (di dunia),",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 31,
				"end": 33
			}
		]
	},
	{
		"aya_id": 5389,
		"aya_number": 14,
		"aya_text": "وَمَنْ فِى الْاَرْضِ جَمِيْعًاۙ ثُمَّ يُنْجِيْهِۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "dan orang-orang di bumi seluruhnya, kemudian mengharapkan (tebusan) itu dapat menyelamatkannya.",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 4,
				"end": 9
			},
			{
				"class": "ikhfa",
				"start": 27,
				"end": 29
			},
			{
				"class": "ikhfa",
				"start": 31,
				"end": 33
			},
			{
				"class": "ghunnah",
				"start": 34,
				"end": 37
			},
			{
				"class": "ikhfa",
				"start": 40,
				"end": 44
			},
			{
				"class": "madd-arid-lissukun",
				"start": 44,
				"end": 46
			}
		]
	},
	{
		"aya_id": 5390,
		"aya_number": 15,
		"aya_text": "كَلَّاۗ اِنَّهَا لَظٰىۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "Sama sekali tidak! Sungguh, neraka itu api yang bergejolak,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 10,
				"end": 13
			}
		]
	},
	{
		"aya_id": 5391,
		"aya_number": 16,
		"aya_text": "نَزَّاعَةً لِّلشَّوٰىۚ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "yang mengelupaskan kulit kepala. ",
		"tajweed": [
			{
				"class": "idgham-bilaghunnah",
				"start": 8,
				"end": 13
			}
		]
	},
	{
		"aya_id": 5392,
		"aya_number": 17,
		"aya_text": "تَدْعُوْا مَنْ اَدْبَرَ وَتَوَلّٰىۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "Yang memanggil orang yang membelakangi dan yang berpaling (dari agama),",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 2,
				"end": 3
			},
			{
				"class": "qalqalah",
				"start": 17,
				"end": 18
			}
		]
	},
	{
		"aya_id": 5393,
		"aya_number": 18,
		"aya_text": "وَجَمَعَ فَاَوْعٰى ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "dan orang yang mengumpulkan (harta benda) lalu menyimpannya. ",
		"tajweed": []
	},
	{
		"aya_id": 5394,
		"aya_number": 19,
		"aya_text": "۞ اِنَّ الْاِنْسَانَ خُلِقَ هَلُوْعًاۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "Sungguh, manusia diciptakan bersifat suka mengeluh. ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			},
			{
				"class": "ikhfa",
				"start": 13,
				"end": 17
			}
		]
	},
	{
		"aya_id": 5395,
		"aya_number": 20,
		"aya_text": "اِذَا مَسَّهُ الشَّرُّ جَزُوْعًاۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "Apabila dia ditimpa kesusahan dia berkeluh kesah, ",
		"tajweed": []
	},
	{
		"aya_id": 5396,
		"aya_number": 21,
		"aya_text": "وَّاِذَا مَسَّهُ الْخَيْرُ مَنُوْعًاۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "dan apabila mendapat kebaikan (harta) dia jadi kikir, ",
		"tajweed": []
	},
	{
		"aya_id": 5397,
		"aya_number": 22,
		"aya_text": "اِلَّا الْمُصَلِّيْنَۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "kecuali orang-orang yang melaksanakan salat, ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 17,
				"end": 19
			}
		]
	},
	{
		"aya_id": 5398,
		"aya_number": 23,
		"aya_text": "الَّذِيْنَ هُمْ عَلٰى صَلَاتِهِمْ دَاۤىِٕمُوْنَۖ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "mereka yang tetap setia melaksanakan salatnya, ",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 35,
				"end": 39
			},
			{
				"class": "madd-arid-lissukun",
				"start": 43,
				"end": 45
			}
		]
	},
	{
		"aya_id": 5399,
		"aya_number": 24,
		"aya_text": "وَالَّذِيْنَ فِيْٓ اَمْوَالِهِمْ حَقٌّ مَّعْلُوْمٌۖ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "dan orang-orang yang dalam hartanya disiapkan bagian tertentu, ",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 15,
				"end": 19
			},
			{
				"class": "idgham-bighunnah",
				"start": 35,
				"end": 41
			},
			{
				"class": "madd-arid-lissukun",
				"start": 46,
				"end": 48
			}
		]
	},
	{
		"aya_id": 5400,
		"aya_number": 25,
		"aya_text": "لِّلسَّاۤىِٕلِ وَالْمَحْرُوْمِۖ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "bagi orang (miskin) yang meminta dan yang tidak meminta, ",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 6,
				"end": 10
			},
			{
				"class": "madd-arid-lissukun",
				"start": 26,
				"end": 28
			}
		]
	},
	{
		"aya_id": 5401,
		"aya_number": 26,
		"aya_text": "وَالَّذِيْنَ يُصَدِّقُوْنَ بِيَوْمِ الدِّيْنِۖ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "dan orang-orang yang mempercayai hari pembalasan, ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 41,
				"end": 43
			}
		]
	},
	{
		"aya_id": 5402,
		"aya_number": 27,
		"aya_text": "وَالَّذِيْنَ هُمْ مِّنْ عَذَابِ رَبِّهِمْ مُّشْفِقُوْنَۚ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "dan orang-orang yang takut terhadap azab Tuhannya, ",
		"tajweed": [
			{
				"class": "idgham-mimi",
				"start": 15,
				"end": 20
			},
			{
				"class": "idgham-mimi",
				"start": 39,
				"end": 44
			},
			{
				"class": "madd-arid-lissukun",
				"start": 51,
				"end": 53
			}
		]
	},
	{
		"aya_id": 5403,
		"aya_number": 28,
		"aya_text": "اِنَّ عَذَابَ رَبِّهِمْ غَيْرُ مَأْمُوْنٍۖ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "sesungguhnya terhadap azab Tuhan mereka, tidak ada seseorang yang merasa aman (dari kedatangannya), ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			},
			{
				"class": "madd-arid-lissukun",
				"start": 37,
				"end": 39
			}
		]
	},
	{
		"aya_id": 5404,
		"aya_number": 29,
		"aya_text": "وَّالَّذِيْنَ هُمْ لِفُرُوْجِهِمْ حٰفِظُوْنَۙ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "dan orang-orang yang memelihara kemaluannya, ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 40,
				"end": 42
			}
		]
	},
	{
		"aya_id": 5405,
		"aya_number": 30,
		"aya_text": "اِلَّا عَلٰٓى اَزْوَاجِهِمْ اَوْ مَا مَلَكَتْ اَيْمَانُهُمْ فَاِنَّهُمْ غَيْرُ مَلُوْمِيْنَۚ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "kecuali terhadap istri-istri mereka atau hamba sahaya yang mereka miliki maka sesungguhnya mereka tidak tercela. ",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 9,
				"end": 14
			},
			{
				"class": "ghunnah",
				"start": 64,
				"end": 67
			},
			{
				"class": "madd-arid-lissukun",
				"start": 87,
				"end": 89
			}
		]
	},
	{
		"aya_id": 5406,
		"aya_number": 31,
		"aya_text": "فَمَنِ ابْتَغٰى وَرَاۤءَ ذٰلِكَ فَاُولٰۤىِٕكَ هُمُ الْعٰدُوْنَۚ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "Maka barangsiapa mencari di luar itu (seperti zina, homoseks dan lesbian), mereka itulah orang-orang yang melampaui batas. ",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 8,
				"end": 9
			},
			{
				"class": "madd-wajib",
				"start": 19,
				"end": 22
			},
			{
				"class": "madd-wajib",
				"start": 37,
				"end": 41
			},
			{
				"class": "madd-arid-lissukun",
				"start": 58,
				"end": 60
			}
		]
	},
	{
		"aya_id": 5407,
		"aya_number": 32,
		"aya_text": "وَالَّذِيْنَ هُمْ لِاَمٰنٰتِهِمْ وَعَهْدِهِمْ رَاعُوْنَۖ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
		"translation_aya_text": "Dan orang-orang yang memelihara amanat dan janjinya, ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 51,
				"end": 53
			}
		]
	},
	{
		"aya_id": 5408,
		"aya_number": 33,
		"aya_text": "وَالَّذِيْنَ هُمْ بِشَهٰدٰتِهِمْ قَاۤىِٕمُوْنَۖ",
		"sura_id": 70,
		"juz_id": 29,
		"page_number": 569,
