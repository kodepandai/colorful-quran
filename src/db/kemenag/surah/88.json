[
	{
		"aya_id": 5968,
		"aya_number": 1,
		"aya_text": "هَلْ اَتٰىكَ حَدِيْثُ الْغَاشِيَةِۗ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "Sudahkah sampai kepadamu berita tentang (hari Kiamat)?"
	},
	{
		"aya_id": 5969,
		"aya_number": 2,
		"aya_text": "وُجُوْهٌ يَّوْمَىِٕذٍ خَاشِعَةٌ  ۙ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "Pada hari itu banyak wajah yang tertunduk terhina,"
	},
	{
		"aya_id": 5970,
		"aya_number": 3,
		"aya_text": "عَامِلَةٌ نَّاصِبَةٌ  ۙ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "(karena) bekerja keras lagi kepayahan,"
	},
	{
		"aya_id": 5971,
		"aya_number": 4,
		"aya_text": "تَصْلٰى نَارًا حَامِيَةً  ۙ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "mereka memasuki api yang sangat panas (neraka),"
	},
	{
		"aya_id": 5972,
		"aya_number": 5,
		"aya_text": "تُسْقٰى مِنْ عَيْنٍ اٰنِيَةٍ ۗ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "diberi minum dari sumber mata air yang sangat panas."
	},
	{
		"aya_id": 5973,
		"aya_number": 6,
		"aya_text": "لَيْسَ لَهُمْ طَعَامٌ اِلَّا مِنْ ضَرِيْعٍۙ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "Tidak ada makanan bagi mereka selain dari pohon yang berduri,"
	},
	{
		"aya_id": 5974,
		"aya_number": 7,
		"aya_text": "لَّا يُسْمِنُ وَلَا يُغْنِيْ مِنْ جُوْعٍۗ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "yang tidak menggemukkan dan tidak menghilangkan lapar."
	},
	{
		"aya_id": 5975,
		"aya_number": 8,
		"aya_text": "وُجُوْهٌ يَّوْمَىِٕذٍ نَّاعِمَةٌ  ۙ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "Pada hari itu banyak (pula) wajah yang berseri-seri,"
	},
	{
		"aya_id": 5976,
		"aya_number": 9,
		"aya_text": "لِّسَعْيِهَا رَاضِيَةٌ  ۙ ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "merasa senang karena usahanya (sendiri),"
	},
	{
		"aya_id": 5977,
		"aya_number": 10,
		"aya_text": "فِيْ جَنَّةٍ عَالِيَةٍۙ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "(mereka) dalam surga yang tinggi, "
	},
	{
		"aya_id": 5978,
		"aya_number": 11,
		"aya_text": "لَّا تَسْمَعُ فِيْهَا لَاغِيَةً ۗ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "di sana (kamu) tidak mendengar perkataan yang tidak berguna."
	},
	{
		"aya_id": 5979,
		"aya_number": 12,
		"aya_text": "فِيْهَا عَيْنٌ جَارِيَةٌ  ۘ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "Di sana ada mata air yang mengalir."
	},
	{
		"aya_id": 5980,
		"aya_number": 13,
		"aya_text": "فِيْهَا سُرُرٌ مَّرْفُوْعَةٌ  ۙ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "Di sana ada dipan-dipan yang ditinggikan,"
	},
	{
		"aya_id": 5981,
		"aya_number": 14,
		"aya_text": "وَّاَكْوَابٌ مَّوْضُوْعَةٌ  ۙ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "dan gelas-gelas yang tersedia (di dekatnya),"
	},
	{
		"aya_id": 5982,
		"aya_number": 15,
		"aya_text": "وَّنَمَارِقُ مَصْفُوْفَةٌ  ۙ ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "dan bantal-bantal sandaran yang tersusun,"
	},
	{
		"aya_id": 5983,
		"aya_number": 16,
		"aya_text": "وَّزَرَابِيُّ مَبْثُوْثَةٌ  ۗ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "dan permadani-permadani yang terhampar."
	},
	{
		"aya_id": 5984,
		"aya_number": 17,
		"aya_text": "اَفَلَا يَنْظُرُوْنَ اِلَى الْاِبِلِ كَيْفَ خُلِقَتْۗ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "Maka tidakkah mereka memperhatikan unta, bagaimana diciptakan?"
	},
	{
		"aya_id": 5985,
		"aya_number": 18,
		"aya_text": "وَاِلَى السَّمَاۤءِ كَيْفَ رُفِعَتْۗ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "dan langit, bagaimana ditinggikan?"
	},
	{
		"aya_id": 5986,
		"aya_number": 19,
		"aya_text": "وَاِلَى الْجِبَالِ كَيْفَ نُصِبَتْۗ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "Dan gunung-gunung bagaimana ditegakkan?"
	},
	{
		"aya_id": 5987,
		"aya_number": 20,
		"aya_text": "وَاِلَى الْاَرْضِ كَيْفَ سُطِحَتْۗ ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "Dan bumi bagaimana dihamparkan?"
	},
	{
		"aya_id": 5988,
		"aya_number": 21,
		"aya_text": "فَذَكِّرْۗ اِنَّمَآ اَنْتَ مُذَكِّرٌۙ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "Maka berilah peringatan, karena sesungguhnya engkau (Muhammad) hanyalah pemberi peringatan."
	},
	{
		"aya_id": 5989,
		"aya_number": 22,
		"aya_text": "لَّسْتَ عَلَيْهِمْ بِمُصَيْطِرٍۙ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "Engkau bukanlah orang yang berkuasa atas mereka,"
	},
	{
		"aya_id": 5990,
		"aya_number": 23,
		"aya_text": "اِلَّا مَنْ تَوَلّٰى وَكَفَرَۙ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "kecuali (jika ada) orang yang berpaling dan kafir,"
	},
	{
		"aya_id": 5991,
		"aya_number": 24,
		"aya_text": "فَيُعَذِّبُهُ اللّٰهُ الْعَذَابَ الْاَكْبَرَۗ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "maka Allah akan mengazabnya dengan azab yang besar."
	},
	{
		"aya_id": 5992,
		"aya_number": 25,
		"aya_text": "اِنَّ اِلَيْنَآ اِيَابَهُمْ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "Sungguh, kepada Kamilah mereka kembali,"
	},
	{
		"aya_id": 5993,
		"aya_number": 26,
		"aya_text": "ثُمَّ اِنَّ عَلَيْنَا حِسَابَهُمْ ࣖ",
		"sura_id": 88,
		"juz_id": 30,
		"page_number": 592,
		"translation_aya_text": "kemudian sesungguhnya (kewajiban) Kamilah membuat perhitungan atas mereka."
	}
]
