[
	{
		"aya_id": 5673,
		"aya_number": 1,
		"aya_text": "عَمَّ يَتَسَاۤءَلُوْنَۚ ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "Tentang apakah mereka saling bertanya-tanya?",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "madd-wajib",
				"start": 12,
				"end": 14
			},
			{
				"class": "madd-arid-lissukun",
				"start": 18,
				"end": 20
			}
		]
	},
	{
		"aya_id": 5674,
		"aya_number": 2,
		"aya_text": "عَنِ النَّبَاِ الْعَظِيْمِۙ ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "Tentang berita yang besar (hari kebangkitan),",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 7,
				"end": 9
			},
			{
				"class": "madd-arid-lissukun",
				"start": 22,
				"end": 24
			}
		]
	},
	{
		"aya_id": 5675,
		"aya_number": 3,
		"aya_text": "الَّذِيْ هُمْ فِيْهِ مُخْتَلِفُوْنَۗ ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "yang dalam hal itu mereka berselisih.",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 31,
				"end": 33
			}
		]
	},
	{
		"aya_id": 5676,
		"aya_number": 4,
		"aya_text": "كَلَّا سَيَعْلَمُوْنَۙ ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "Tidak! Kelak mereka akan mengetahui,",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 17,
				"end": 19
			}
		]
	},
	{
		"aya_id": 5677,
		"aya_number": 5,
		"aya_text": "ثُمَّ كَلَّا سَيَعْلَمُوْنَ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "sekali lagi tidak! Kelak mereka akan mengetahui. ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "madd-arid-lissukun",
				"start": 23,
				"end": 25
			}
		]
	},
	{
		"aya_id": 5678,
		"aya_number": 6,
		"aya_text": "اَلَمْ نَجْعَلِ الْاَرْضَ مِهٰدًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "Bukankah Kami telah menjadikan bumi sebagai hamparan, ",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 9,
				"end": 10
			}
		]
	},
	{
		"aya_id": 5679,
		"aya_number": 7,
		"aya_text": "وَّالْجِبَالَ اَوْتَادًاۖ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "dan gunung-gunung sebagai pasak?",
		"tajweed": []
	},
	{
		"aya_id": 5680,
		"aya_number": 8,
		"aya_text": "وَّخَلَقْنٰكُمْ اَزْوَاجًاۙ ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "Dan Kami menciptakan kamu berpasang-pasangan,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 7,
				"end": 8
			}
		]
	},
	{
		"aya_id": 5681,
		"aya_number": 9,
		"aya_text": "وَّجَعَلْنَا نَوْمَكُمْ سُبَاتًاۙ ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "dan Kami menjadikan tidurmu untuk istirahat,",
		"tajweed": []
	},
	{
		"aya_id": 5682,
		"aya_number": 10,
		"aya_text": "وَّجَعَلْنَا الَّيْلَ لِبَاسًاۙ ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "dan Kami menjadikan malam sebagai pakaian,",
		"tajweed": []
	},
	{
		"aya_id": 5683,
		"aya_number": 11,
		"aya_text": "وَّجَعَلْنَا النَّهَارَ مَعَاشًاۚ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "dan Kami menjadikan siang untuk mencari penghidupan,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 15,
				"end": 17
			}
		]
	},
	{
		"aya_id": 5684,
		"aya_number": 12,
		"aya_text": "وَبَنَيْنَا فَوْقَكُمْ سَبْعًا شِدَادًاۙ ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "dan Kami membangun di atas kamu tujuh (langit) yang kokoh,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 25,
				"end": 26
			},
			{
				"class": "ikhfa",
				"start": 27,
				"end": 29
			},
			{
				"class": "ikhfa",
				"start": 30,
				"end": 32
			}
		]
	},
	{
		"aya_id": 5685,
		"aya_number": 13,
		"aya_text": "وَّجَعَلْنَا سِرَاجًا وَّهَّاجًاۖ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "dan Kami menjadikan pelita yang terang-benderang (matahari),",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 18,
				"end": 20
			},
			{
				"class": "idgham-bighunnah",
				"start": 21,
				"end": 23
			}
		]
	},
	{
		"aya_id": 5686,
		"aya_number": 14,
		"aya_text": "وَّاَنْزَلْنَا مِنَ الْمُعْصِرٰتِ مَاۤءً ثَجَّاجًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "dan Kami turunkan dari awan, air hujan yang tercurah dengan hebatnya,",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 5,
				"end": 9
			},
			{
				"class": "madd-wajib",
				"start": 36,
				"end": 38
			},
			{
				"class": "ikhfa",
				"start": 38,
				"end": 43
			}
		]
	},
	{
		"aya_id": 5687,
		"aya_number": 15,
		"aya_text": "لِّنُخْرِجَ بِهٖ حَبًّا وَّنَبَاتًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "untuk Kami tumbuhkan dengan air itu biji-bijian dan tanam-tanaman,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 19,
				"end": 22
			},
			{
				"class": "idgham-bighunnah",
				"start": 23,
				"end": 25
			}
		]
	},
	{
		"aya_id": 5688,
		"aya_number": 16,
		"aya_text": "وَّجَنّٰتٍ اَلْفَافًاۗ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "dan kebun-kebun yang rindang.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 5,
				"end": 7
			}
		]
	},
	{
		"aya_id": 5689,
		"aya_number": 17,
		"aya_text": "اِنَّ يَوْمَ الْفَصْلِ كَانَ مِيْقَاتًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "Sungguh, hari keputusan adalah suatu waktu yang telah ditetapkan,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			}
		]
	},
	{
		"aya_id": 5690,
		"aya_number": 18,
		"aya_text": "يَّوْمَ يُنْفَخُ فِى الصُّوْرِ فَتَأْتُوْنَ اَفْوَاجًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "(yaitu) pada hari (ketika) sangkakala ditiup, lalu kamu datang berbondong-bondong,",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 10,
				"end": 14
			}
		]
	},
	{
		"aya_id": 5691,
		"aya_number": 19,
		"aya_text": "وَّفُتِحَتِ السَّمَاۤءُ فَكَانَتْ اَبْوَابًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "dan langit pun dibukalah, maka terdapatlah beberapa pintu,",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 19,
				"end": 21
			},
			{
				"class": "qalqalah",
				"start": 36,
				"end": 37
			}
		]
	},
	{
		"aya_id": 5692,
		"aya_number": 20,
		"aya_text": "وَّسُيِّرَتِ الْجِبَالُ فَكَانَتْ سَرَابًاۗ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "dan gunung-gunung pun dijalankan sehingga menjadi fatamorgana.",
		"tajweed": []
	},
	{
		"aya_id": 5693,
		"aya_number": 21,
		"aya_text": "اِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "Sungguh, (neraka) Jahanam itu (sebagai) tempat mengintai (bagi penjaga yang mengawasi isi neraka), ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "ghunnah",
				"start": 10,
				"end": 12
			}
		]
	},
	{
		"aya_id": 5694,
		"aya_number": 22,
		"aya_text": "لِّلطّٰغِيْنَ مَاٰبًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "menjadi tempat kembali bagi orang-orang yang melampaui batas.",
		"tajweed": []
	},
	{
		"aya_id": 5695,
		"aya_number": 23,
		"aya_text": "لّٰبِثِيْنَ فِيْهَآ اَحْقَابًاۚ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "Mereka tinggal di sana dalam masa yang lama,",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 18,
				"end": 21
			}
		]
	},
	{
		"aya_id": 5696,
		"aya_number": 24,
		"aya_text": "لَا يَذُوْقُوْنَ فِيْهَا بَرْدًا وَّلَا شَرَابًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "mereka tidak merasakan kesejukan di dalamnya dan tidak (pula mendapat) minuman,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 29,
				"end": 31
			},
			{
				"class": "idgham-bighunnah",
				"start": 32,
				"end": 34
			}
		]
	},
	{
		"aya_id": 5697,
		"aya_number": 25,
		"aya_text": "اِلَّا حَمِيْمًا وَّغَسَّاقًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "selain air yang mendidih dan nanah,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 13,
				"end": 15
			},
			{
				"class": "idgham-bighunnah",
				"start": 16,
				"end": 18
			}
		]
	},
	{
		"aya_id": 5698,
		"aya_number": 26,
		"aya_text": "جَزَاۤءً وِّفَاقًاۗ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "sebagai pembalasan yang setimpal.",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 4,
				"end": 6
			},
			{
				"class": "idgham-bighunnah",
				"start": 6,
				"end": 11
			}
		]
	},
	{
		"aya_id": 5699,
		"aya_number": 27,
		"aya_text": "اِنَّهُمْ كَانُوْا لَا يَرْجُوْنَ حِسَابًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "Sesungguhnya dahulu mereka tidak pernah mengharapkan perhitungan.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			}
		]
	},
	{
		"aya_id": 5700,
		"aya_number": 28,
		"aya_text": "وَّكَذَّبُوْا بِاٰيٰتِنَا كِذَّابًاۗ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "Dan mereka benar-benar mendustakan ayat-ayat Kami.",
		"tajweed": []
	},
	{
		"aya_id": 5701,
		"aya_number": 29,
		"aya_text": "وَكُلَّ شَيْءٍ اَحْصَيْنٰهُ كِتٰبًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "Dan segala sesuatu telah Kami catat dalam suatu Kitab (buku catatan amalan manusia). ",
		"tajweed": []
	},
	{
		"aya_id": 5702,
		"aya_number": 30,
		"aya_text": "فَذُوْقُوْا فَلَنْ نَّزِيْدَكُمْ اِلَّا عَذَابًا ࣖ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 582,
		"translation_aya_text": "Maka karena itu rasakanlah! Maka tidak ada yang akan Kami tambahkan kepadamu selain azab.",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 16,
				"end": 21
			}
		]
	},
	{
		"aya_id": 5703,
		"aya_number": 31,
		"aya_text": "اِنَّ لِلْمُتَّقِيْنَ مَفَازًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "Sungguh, orang-orang yang bertakwa mendapat kemenangan,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			}
		]
	},
	{
		"aya_id": 5704,
		"aya_number": 32,
		"aya_text": "حَدَاۤىِٕقَ وَاَعْنَابًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "(yaitu) kebun-kebun dan buah anggur,",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 4,
				"end": 7
			}
		]
	},
	{
		"aya_id": 5705,
		"aya_number": 33,
		"aya_text": "وَّكَوَاعِبَ اَتْرَابًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "dan gadis-gadis montok yang sebaya,",
		"tajweed": []
	},
	{
		"aya_id": 5706,
		"aya_number": 34,
		"aya_text": "وَّكَأْسًا دِهَاقًاۗ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "dan gelas-gelas yang penuh (berisi minuman).",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 7,
				"end": 9
			},
			{
				"class": "ikhfa",
				"start": 10,
				"end": 12
			}
		]
	},
	{
		"aya_id": 5707,
		"aya_number": 35,
		"aya_text": "لَا يَسْمَعُوْنَ فِيْهَا لَغْوًا وَّلَا كِذَّابًا",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "Di sana mereka tidak mendengar percakapan yang sia-sia maupun (perkataan) dusta.",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 29,
				"end": 31
			},
			{
				"class": "idgham-bighunnah",
				"start": 32,
				"end": 34
			},
			{
				"class": "madd-arid-lissukun",
				"start": 45,
				"end": 46
			}
		]
	},
	{
		"aya_id": 5708,
		"aya_number": 36,
		"aya_text": "جَزَاۤءً مِّنْ رَّبِّكَ عَطَاۤءً حِسَابًاۙ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "Sebagai balasan dan pemberian yang cukup banyak dari Tuhanmu,",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 4,
				"end": 6
			},
			{
				"class": "idgham-bighunnah",
				"start": 6,
				"end": 11
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 12,
				"end": 17
			},
			{
				"class": "madd-wajib",
				"start": 28,
				"end": 30
			}
		]
	},
	{
		"aya_id": 5709,
		"aya_number": 37,
		"aya_text": "رَّبِّ السَّمٰوٰتِ وَالْاَرْضِ وَمَا بَيْنَهُمَا الرَّحْمٰنِ لَا يَمْلِكُوْنَ مِنْهُ خِطَابًاۚ ",
		"sura_id": 78,
		"juz_id": 30,
		"page_number": 583,
