	},
	{
		"aya_id": 5317,
		"aya_number": 46,
		"aya_text": "اَمْ تَسْـَٔلُهُمْ اَجْرًا فَهُمْ مِّنْ مَّغْرَمٍ مُّثْقَلُوْنَۚ ",
		"sura_id": 68,
		"juz_id": 29,
		"page_number": 566,
