[
	{
		"aya_id": 6059,
		"aya_number": 1,
		"aya_text": "وَالَّيْلِ اِذَا يَغْشٰىۙ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "Demi malam apabila menutupi (cahaya siang),",
		"tajweed": []
	},
	{
		"aya_id": 6060,
		"aya_number": 2,
		"aya_text": "وَالنَّهَارِ اِذَا تَجَلّٰىۙ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "demi siang apabila terang benderang,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 6
			}
		]
	},
	{
		"aya_id": 6061,
		"aya_number": 3,
		"aya_text": "وَمَا خَلَقَ الذَّكَرَ وَالْاُنْثٰىٓ ۙ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "demi penciptaan laki-laki dan perempuan,",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 30,
				"end": 34
			}
		]
	},
	{
		"aya_id": 6062,
		"aya_number": 4,
		"aya_text": "اِنَّ سَعْيَكُمْ لَشَتّٰىۗ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "sungguh, usahamu memang beraneka macam.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			}
		]
	},
	{
		"aya_id": 6063,
		"aya_number": 5,
		"aya_text": "فَاَمَّا مَنْ اَعْطٰى وَاتَّقٰىۙ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "Maka barangsiapa memberikan (hartanya di jalan Allah) dan bertakwa,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 6
			}
		]
	},
	{
		"aya_id": 6064,
		"aya_number": 6,
		"aya_text": "وَصَدَّقَ بِالْحُسْنٰىۙ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "dan membenarkan (adanya pahala) yang terbaik (surga),",
		"tajweed": []
	},
	{
		"aya_id": 6065,
		"aya_number": 7,
		"aya_text": "فَسَنُيَسِّرُهٗ لِلْيُسْرٰىۗ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "maka akan Kami mudahkan baginya jalan menuju kemudahan (kebahagiaan),",
		"tajweed": []
	},
	{
		"aya_id": 6066,
		"aya_number": 8,
		"aya_text": "وَاَمَّا مَنْۢ بَخِلَ وَاسْتَغْنٰىۙ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "dan adapun orang yang kikir dan merasa dirinya cukup (tidak perlu pertolongan Allah),",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 6
			},
			{
				"class": "iqlab",
				"start": 11,
				"end": 13
			},
			{
				"class": "iqlab",
				"start": 14,
				"end": 16
			}
		]
	},
	{
		"aya_id": 6067,
		"aya_number": 9,
		"aya_text": "وَكَذَّبَ بِالْحُسْنٰىۙ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "serta mendustakan (pahala) yang terbaik,",
		"tajweed": []
	},
	{
		"aya_id": 6068,
		"aya_number": 10,
		"aya_text": "فَسَنُيَسِّرُهٗ لِلْعُسْرٰىۗ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "maka akan Kami mudahkan baginya jalan menuju kesukaran (kesengsaraan),",
		"tajweed": []
	},
	{
		"aya_id": 6069,
		"aya_number": 11,
		"aya_text": "وَمَا يُغْنِيْ عَنْهُ مَالُهٗٓ اِذَا تَرَدّٰىٓۙ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "dan hartanya tidak bermanfaat baginya apabila dia telah binasa.",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 27,
				"end": 31
			}
		]
	},
	{
		"aya_id": 6070,
		"aya_number": 12,
		"aya_text": "اِنَّ عَلَيْنَا لَلْهُدٰىۖ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "Sesungguhnya Kamilah yang memberi petunjuk,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			}
		]
	},
	{
		"aya_id": 6071,
		"aya_number": 13,
		"aya_text": "وَاِنَّ لَنَا لَلْاٰخِرَةَ وَالْاُوْلٰىۗ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "dan sesungguhnya milik Kamilah akhirat dan dunia itu.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 6
			},
			{
				"class": "madd-arid-lissukun",
				"start": 34,
				"end": 36
			}
		]
	},
	{
		"aya_id": 6072,
		"aya_number": 14,
		"aya_text": "فَاَنْذَرْتُكُمْ نَارًا تَلَظّٰىۚ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "Maka Aku memperingatkan kamu dengan neraka yang menyala-nyala,",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 4,
				"end": 8
			},
			{
				"class": "ikhfa",
				"start": 20,
				"end": 22
			},
			{
				"class": "ikhfa",
				"start": 23,
				"end": 25
			}
		]
	},
	{
		"aya_id": 6073,
		"aya_number": 15,
		"aya_text": "لَا يَصْلٰىهَآ اِلَّا الْاَشْقَىۙ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 596,
		"translation_aya_text": "yang hanya dimasuki oleh orang yang paling celaka,",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 13,
				"end": 16
			}
		]
	},
	{
		"aya_id": 6074,
		"aya_number": 16,
		"aya_text": "الَّذِيْ كَذَّبَ وَتَوَلّٰىۗ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 596,
		"translation_aya_text": "yang mendustakan (kebenaran) dan berpaling (dari iman).",
		"tajweed": []
	},
	{
		"aya_id": 6075,
		"aya_number": 17,
		"aya_text": "وَسَيُجَنَّبُهَا الْاَتْقَىۙ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 596,
		"translation_aya_text": "Dan akan dijauhkan darinya (neraka) orang yang paling bertakwa, ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 8,
				"end": 10
			}
		]
	},
	{
		"aya_id": 6076,
		"aya_number": 18,
		"aya_text": "الَّذِيْ يُؤْتِيْ مَالَهٗ يَتَزَكّٰىۚ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 596,
		"translation_aya_text": "yang menginfakkan hartanya (di jalan Allah) untuk membersihkan (dirinya),",
		"tajweed": []
	},
	{
		"aya_id": 6077,
		"aya_number": 19,
		"aya_text": "وَمَا لِاَحَدٍ عِنْدَهٗ مِنْ نِّعْمَةٍ تُجْزٰىٓۙ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 596,
		"translation_aya_text": "dan tidak ada seorang pun memberikan suatu nikmat padanya yang harus dibalasnya,",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 17,
				"end": 21
			},
			{
				"class": "idgham-bighunnah",
				"start": 26,
				"end": 31
			},
			{
				"class": "ikhfa",
				"start": 36,
				"end": 41
			},
			{
				"class": "qalqalah",
				"start": 41,
				"end": 42
			}
		]
	},
	{
		"aya_id": 6078,
		"aya_number": 20,
		"aya_text": "اِلَّا ابْتِغَاۤءَ وَجْهِ رَبِّهِ الْاَعْلٰىۚ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 596,
		"translation_aya_text": "tetapi (dia memberikan itu semata-mata) karena mencari keridaan Tuhannya Yang Mahatinggi.",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 8,
				"end": 9
			},
			{
				"class": "madd-wajib",
				"start": 14,
				"end": 16
			},
			{
				"class": "qalqalah",
				"start": 21,
				"end": 22
			}
		]
	},
	{
		"aya_id": 6079,
		"aya_number": 21,
		"aya_text": "وَلَسَوْفَ يَرْضٰى ࣖ",
		"sura_id": 92,
		"juz_id": 30,
		"page_number": 596,
		"translation_aya_text": "Dan niscaya kelak dia akan mendapat kesenangan (yang sempurna).",
		"tajweed": []
	}
]
