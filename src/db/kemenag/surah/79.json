[
	{
		"aya_id": 5713,
		"aya_number": 1,
		"aya_text": "وَالنّٰزِعٰتِ غَرْقًاۙ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "Demi (malaikat) yang mencabut (nyawa) dengan keras.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 6
			}
		]
	},
	{
		"aya_id": 5714,
		"aya_number": 2,
		"aya_text": "وَّالنّٰشِطٰتِ نَشْطًاۙ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "Demi (malaikat) yang mencabut (nyawa) dengan lemah lembut.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 5,
				"end": 7
			}
		]
	},
	{
		"aya_id": 5715,
		"aya_number": 3,
		"aya_text": "وَّالسّٰبِحٰتِ سَبْحًاۙ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "Demi (malaikat) yang turun dari langit dengan cepat,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 17,
				"end": 18
			}
		]
	},
	{
		"aya_id": 5716,
		"aya_number": 4,
		"aya_text": "فَالسّٰبِقٰتِ سَبْقًاۙ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "dan (malaikat) yang mendahului dengan kencang,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 16,
				"end": 17
			}
		]
	},
	{
		"aya_id": 5717,
		"aya_number": 5,
		"aya_text": "فَالْمُدَبِّرٰتِ اَمْرًاۘ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "dan (malaikat) yang mengatur urusan (dunia).",
		"tajweed": []
	},
	{
		"aya_id": 5718,
		"aya_number": 6,
		"aya_text": "يَوْمَ تَرْجُفُ الرَّاجِفَةُۙ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "(Sungguh, kamu akan dibangkitkan) pada hari ketika tiupan pertama mengguncangkan alam,",
		"tajweed": []
	},
	{
		"aya_id": 5719,
		"aya_number": 7,
		"aya_text": "تَتْبَعُهَا الرَّادِفَةُ ۗ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "(tiupan pertama) itu diiringi oleh tiupan kedua.",
		"tajweed": []
	},
	{
		"aya_id": 5720,
		"aya_number": 8,
		"aya_text": "قُلُوْبٌ يَّوْمَىِٕذٍ وَّاجِفَةٌۙ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "Hati manusia pada waktu itu merasa sangat takut,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 6,
				"end": 11
			},
			{
				"class": "idgham-bighunnah",
				"start": 19,
				"end": 24
			}
		]
	},
	{
		"aya_id": 5721,
		"aya_number": 9,
		"aya_text": "اَبْصَارُهَا خَاشِعَةٌ  ۘ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "pandangannya tunduk.",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 2,
				"end": 3
			}
		]
	},
	{
		"aya_id": 5722,
		"aya_number": 10,
		"aya_text": "يَقُوْلُوْنَ ءَاِنَّا لَمَرْدُوْدُوْنَ فِى الْحَافِرَةِۗ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "(Orang-orang kafir) berkata, “Apakah kita benar-benar akan dikembalikan kepada kehidupan yang semula?   ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 17,
				"end": 19
			}
		]
	},
	{
		"aya_id": 5723,
		"aya_number": 11,
		"aya_text": "ءَاِذَا كُنَّا عِظَامًا نَّخِرَةً ۗ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "Apakah (akan dibangkitkan juga) apabila kita telah menjadi tulang belulang yang hancur?”",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 10,
				"end": 12
			},
			{
				"class": "idgham-bighunnah",
				"start": 20,
				"end": 22
			},
			{
				"class": "idgham-bighunnah",
				"start": 23,
				"end": 25
			}
		]
	},
	{
		"aya_id": 5724,
		"aya_number": 12,
		"aya_text": "قَالُوْا تِلْكَ اِذًا كَرَّةٌ خَاسِرَةٌ  ۘ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "Mereka berkata, “Kalau demikian, itu adalah suatu pengembalian yang merugikan.”",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 18,
				"end": 20
			},
			{
				"class": "ikhfa",
				"start": 21,
				"end": 23
			}
		]
	},
	{
		"aya_id": 5725,
		"aya_number": 13,
		"aya_text": "فَاِنَّمَا هِيَ زَجْرَةٌ وَّاحِدَةٌۙ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "Maka pengembalian itu hanyalah dengan sekali tiupan saja.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 6
			},
			{
				"class": "qalqalah",
				"start": 18,
				"end": 19
			},
			{
				"class": "idgham-bighunnah",
				"start": 22,
				"end": 27
			}
		]
	},
	{
		"aya_id": 5726,
		"aya_number": 14,
		"aya_text": "فَاِذَا هُمْ بِالسَّاهِرَةِۗ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "Maka seketika itu mereka hidup kembali di bumi (yang baru).",
		"tajweed": [
			{
				"class": "ikhfa-syafawi",
				"start": 10,
				"end": 15
			}
		]
	},
	{
		"aya_id": 5727,
		"aya_number": 15,
		"aya_text": "هَلْ اَتٰىكَ حَدِيْثُ مُوْسٰىۘ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 583,
		"translation_aya_text": "Sudahkah sampai kepadamu (Muhammad) kisah Musa?",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 24,
				"end": 26
			}
		]
	},
	{
		"aya_id": 5728,
		"aya_number": 16,
		"aya_text": "اِذْ نَادٰىهُ رَبُّهٗ بِالْوَادِ الْمُقَدَّسِ طُوًىۚ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "Ketika Tuhan memanggilnya (Musa) di lembah suci yaitu Lembah Tuwa;",
		"tajweed": []
	},
	{
		"aya_id": 5729,
		"aya_number": 17,
		"aya_text": "اِذْهَبْ اِلٰى فِرْعَوْنَ اِنَّهٗ طَغٰىۖ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "pergilah engkau kepada Fir‘aun! Sesungguhnya dia telah melampaui batas,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 6,
				"end": 7
			},
			{
				"class": "ghunnah",
				"start": 28,
				"end": 30
			}
		]
	},
	{
		"aya_id": 5730,
		"aya_number": 18,
		"aya_text": "فَقُلْ هَلْ لَّكَ اِلٰٓى اَنْ تَزَكّٰىۙ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "Maka katakanlah (kepada Fir‘aun), “Adakah keinginanmu untuk membersihkan diri (dari kesesatan),",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 20,
				"end": 25
			},
			{
				"class": "ikhfa",
				"start": 27,
				"end": 32
			}
		]
	},
	{
		"aya_id": 5731,
		"aya_number": 19,
		"aya_text": "وَاَهْدِيَكَ اِلٰى رَبِّكَ فَتَخْشٰىۚ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "dan engkau akan kupimpin ke jalan Tuhanmu agar engkau takut kepada-Nya?”",
		"tajweed": []
	},
	{
		"aya_id": 5732,
		"aya_number": 20,
		"aya_text": "فَاَرٰىهُ الْاٰيَةَ الْكُبْرٰىۖ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "Lalu (Musa) memperlihatkan kepadanya mukjizat yang besar.",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 25,
				"end": 26
			}
		]
	},
	{
		"aya_id": 5733,
		"aya_number": 21,
		"aya_text": "فَكَذَّبَ وَعَصٰىۖ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "Tetapi dia (Fir‘aun) mendustakan dan mendurhakai.",
		"tajweed": []
	},
	{
		"aya_id": 5734,
		"aya_number": 22,
		"aya_text": "ثُمَّ اَدْبَرَ يَسْعٰىۖ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "Kemudian dia berpaling seraya berusaha menantang (Musa).",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "qalqalah",
				"start": 8,
				"end": 9
			}
		]
	},
	{
		"aya_id": 5735,
		"aya_number": 23,
		"aya_text": "فَحَشَرَ فَنَادٰىۖ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "Kemudian dia mengumpulkan (pembesar-pembesarnya) lalu berseru (memanggil kaumnya).",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 13,
				"end": 14
			}
		]
	},
	{
		"aya_id": 5736,
		"aya_number": 24,
		"aya_text": "فَقَالَ اَنَا۠ رَبُّكُمُ الْاَعْلٰىۖ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "(Seraya) berkata, “Akulah tuhanmu yang paling tinggi.”",
		"tajweed": []
	},
	{
		"aya_id": 5737,
		"aya_number": 25,
		"aya_text": "فَاَخَذَهُ اللّٰهُ نَكَالَ الْاٰخِرَةِ وَالْاُوْلٰىۗ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "Maka Allah menghukumnya dengan azab di akhirat dan siksaan di dunia.",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 46,
				"end": 48
			}
		]
	},
	{
		"aya_id": 5738,
		"aya_number": 26,
		"aya_text": "اِنَّ فِيْ ذٰلِكَ لَعِبْرَةً لِّمَنْ يَّخْشٰى ۗ ࣖ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "Sungguh, pada yang demikian itu terdapat pelajaran bagi orang yang takut (kepada Allah).",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "qalqalah",
				"start": 22,
				"end": 23
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 26,
				"end": 31
			},
			{
				"class": "idgham-bighunnah",
				"start": 34,
				"end": 39
			}
		]
	},
	{
		"aya_id": 5739,
		"aya_number": 27,
		"aya_text": "ءَاَنْتُمْ اَشَدُّ خَلْقًا اَمِ السَّمَاۤءُ ۚ بَنٰىهَاۗ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "Apakah penciptaan kamu yang lebih hebat ataukah langit yang telah dibangun-Nya?",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 4,
				"end": 8
			},
			{
				"class": "madd-wajib",
				"start": 39,
				"end": 41
			}
		]
	},
	{
		"aya_id": 5740,
		"aya_number": 28,
		"aya_text": "رَفَعَ سَمْكَهَا فَسَوّٰىهَاۙ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "Dia telah meninggikan bangunannya lalu menyempurnakannya,",
		"tajweed": []
	},
	{
		"aya_id": 5741,
		"aya_number": 29,
		"aya_text": "وَاَغْطَشَ لَيْلَهَا وَاَخْرَجَ ضُحٰىهَاۖ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "dan Dia menjadikan malamnya (gelap gulita), dan menjadikan siangnya (terang benderang).",
		"tajweed": []
	},
	{
		"aya_id": 5742,
		"aya_number": 30,
		"aya_text": "وَالْاَرْضَ بَعْدَ ذٰلِكَ دَحٰىهَاۗ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "Dan setelah itu bumi Dia hamparkan. ",
		"tajweed": []
	},
	{
		"aya_id": 5743,
		"aya_number": 31,
		"aya_text": "اَخْرَجَ مِنْهَا مَاۤءَهَا وَمَرْعٰىهَاۖ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "Darinya Dia pancarkan mata air, dan (ditumbuhkan) tumbuh-tumbuhannya.",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 19,
				"end": 21
			}
		]
	},
	{
		"aya_id": 5744,
		"aya_number": 32,
		"aya_text": "وَالْجِبَالَ اَرْسٰىهَاۙ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "Dan gunung-gunung Dia pancangkan dengan teguh.",
		"tajweed": []
	},
	{
		"aya_id": 5745,
		"aya_number": 33,
		"aya_text": "مَتَاعًا لَّكُمْ وَلِاَنْعَامِكُمْۗ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "(Semua itu) untuk kesenanganmu dan untuk hewan-hewan ternakmu.",
		"tajweed": [
			{
				"class": "idgham-bilaghunnah",
				"start": 5,
				"end": 7
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 8,
				"end": 10
			}
		]
	},
	{
		"aya_id": 5746,
		"aya_number": 34,
		"aya_text": "فَاِذَا جَاۤءَتِ الطَّاۤمَّةُ الْكُبْرٰىۖ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "Maka apabila malapetaka besar (hari Kiamat) telah datang,",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 10,
				"end": 12
			},
			{
				"class": "ghunnah",
				"start": 24,
				"end": 26
			},
			{
				"class": "qalqalah",
				"start": 35,
				"end": 36
			}
		]
	},
	{
		"aya_id": 5747,
		"aya_number": 35,
		"aya_text": "يَوْمَ يَتَذَكَّرُ الْاِنْسَانُ مَا سَعٰىۙ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "yaitu pada hari (ketika) manusia teringat akan apa yang telah dikerjakannya,",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 24,
				"end": 28
			}
		]
	},
	{
		"aya_id": 5748,
		"aya_number": 36,
		"aya_text": "وَبُرِّزَتِ الْجَحِيْمُ لِمَنْ يَّرٰى",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "dan neraka diperlihatkan dengan jelas kepada setiap orang yang melihat.",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 28,
				"end": 33
			}
		]
	},
	{
		"aya_id": 5749,
		"aya_number": 37,
		"aya_text": "فَاَمَّا مَنْ طَغٰىۖ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "Maka adapun orang yang melampaui batas,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 6
			},
			{
				"class": "ikhfa",
				"start": 11,
				"end": 16
			}
		]
	},
	{
		"aya_id": 5750,
		"aya_number": 38,
		"aya_text": "وَاٰثَرَ الْحَيٰوةَ الدُّنْيَاۙ",
		"sura_id": 79,
		"juz_id": 30,
		"page_number": 584,
		"translation_aya_text": "dan lebih mengutamakan kehidupan dunia,",
		"tajweed": [
			{
