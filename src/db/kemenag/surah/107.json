[
	{
		"aya_id": 6198,
		"aya_number": 1,
		"aya_text": "اَرَءَيْتَ الَّذِيْ يُكَذِّبُ بِالدِّيْنِۗ",
		"sura_id": 107,
		"juz_id": 30,
		"page_number": 602,
		"translation_aya_text": "Tahukah kamu (orang) yang mendustakan agama?"
	},
	{
		"aya_id": 6199,
		"aya_number": 2,
		"aya_text": "فَذٰلِكَ الَّذِيْ يَدُعُّ الْيَتِيْمَۙ",
		"sura_id": 107,
		"juz_id": 30,
		"page_number": 602,
		"translation_aya_text": "Maka itulah orang yang menghardik anak yatim,"
	},
	{
		"aya_id": 6200,
		"aya_number": 3,
		"aya_text": "وَلَا يَحُضُّ عَلٰى طَعَامِ الْمِسْكِيْنِۗ",
		"sura_id": 107,
		"juz_id": 30,
		"page_number": 602,
		"translation_aya_text": "dan tidak mendorong memberi makan orang miskin."
	},
	{
		"aya_id": 6201,
		"aya_number": 4,
		"aya_text": "فَوَيْلٌ لِّلْمُصَلِّيْنَۙ",
		"sura_id": 107,
		"juz_id": 30,
		"page_number": 602,
		"translation_aya_text": "Maka celakalah orang yang salat,"
	},
	{
		"aya_id": 6202,
		"aya_number": 5,
		"aya_text": "الَّذِيْنَ هُمْ عَنْ صَلَاتِهِمْ سَاهُوْنَۙ",
		"sura_id": 107,
		"juz_id": 30,
		"page_number": 602,
		"translation_aya_text": "(yaitu) orang-orang yang lalai terhadap salatnya,"
	},
	{
		"aya_id": 6203,
		"aya_number": 6,
		"aya_text": "الَّذِيْنَ هُمْ يُرَاۤءُوْنَۙ",
		"sura_id": 107,
		"juz_id": 30,
		"page_number": 602,
		"translation_aya_text": "yang berbuat ria,"
	},
	{
		"aya_id": 6204,
		"aya_number": 7,
		"aya_text": "وَيَمْنَعُوْنَ الْمَاعُوْنَ ࣖ",
		"sura_id": 107,
		"juz_id": 30,
		"page_number": 602,
		"translation_aya_text": "dan enggan (memberikan) bantuan."
	}
]
