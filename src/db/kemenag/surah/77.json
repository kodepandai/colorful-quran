[
	{
		"aya_id": 5623,
		"aya_number": 1,
		"aya_text": "وَالْمُرْسَلٰتِ عُرْفًاۙ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "Demi (malaikat-malaikat) yang diutus untuk membawa kebaikan,",
		"tajweed": []
	},
	{
		"aya_id": 5624,
		"aya_number": 2,
		"aya_text": "فَالْعٰصِفٰتِ عَصْفًاۙ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "dan (malaikat-malaikat) yang terbang dengan kencangnya,",
		"tajweed": []
	},
	{
		"aya_id": 5625,
		"aya_number": 3,
		"aya_text": "وَّالنّٰشِرٰتِ نَشْرًاۙ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "dan (malaikat-malaikat) yang menyebarkan (rahmat Allah) dengan seluas-luasnya,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 5,
				"end": 7
			}
		]
	},
	{
		"aya_id": 5626,
		"aya_number": 4,
		"aya_text": "فَالْفٰرِقٰتِ فَرْقًاۙ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "dan (malaikat-malaikat) yang membedakan (antara yang baik dan yang buruk) dengan sejelas-jelasnya,",
		"tajweed": []
	},
	{
		"aya_id": 5627,
		"aya_number": 5,
		"aya_text": "فَالْمُلْقِيٰتِ ذِكْرًاۙ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "dan (malaikat-malaikat) yang menyampaikan wahyu,",
		"tajweed": []
	},
	{
		"aya_id": 5628,
		"aya_number": 6,
		"aya_text": "عُذْرًا اَوْ نُذْرًاۙ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "untuk menolak alasan-alasan atau memberi peringatan.",
		"tajweed": []
	},
	{
		"aya_id": 5629,
		"aya_number": 7,
		"aya_text": "اِنَّمَا تُوْعَدُوْنَ لَوَاقِعٌۗ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "Sungguh, apa yang dijanjikan kepadamu pasti terjadi.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			}
		]
	},
	{
		"aya_id": 5630,
		"aya_number": 8,
		"aya_text": "فَاِذَا النُّجُوْمُ طُمِسَتْۙ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "Maka apabila bintang-bintang dihapuskan,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 10,
				"end": 12
			}
		]
	},
	{
		"aya_id": 5631,
		"aya_number": 9,
		"aya_text": "وَاِذَا السَّمَاۤءُ فُرِجَتْۙ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "dan apabila langit terbelah,",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 15,
				"end": 17
			}
		]
	},
	{
		"aya_id": 5632,
		"aya_number": 10,
		"aya_text": "وَاِذَا الْجِبَالُ نُسِفَتْۙ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "dan apabila gunung-gunung dihancurkan menjadi debu,",
		"tajweed": []
	},
	{
		"aya_id": 5633,
		"aya_number": 11,
		"aya_text": "وَاِذَا الرُّسُلُ اُقِّتَتْۗ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "dan apabila rasul-rasul telah ditetapkan waktunya.",
		"tajweed": []
	},
	{
		"aya_id": 5634,
		"aya_number": 12,
		"aya_text": "لِاَيِّ يَوْمٍ اُجِّلَتْۗ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "(Niscaya dikatakan kepada mereka), “Sampai hari apakah ditangguhkan (azab orang-orang kafir itu)?”",
		"tajweed": []
	},
	{
		"aya_id": 5635,
		"aya_number": 13,
		"aya_text": "لِيَوْمِ الْفَصْلِۚ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "Sampai hari keputusan.",
		"tajweed": []
	},
	{
		"aya_id": 5636,
		"aya_number": 14,
		"aya_text": "وَمَآ اَدْرٰىكَ مَا يَوْمُ الْفَصْلِۗ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "Dan tahukah kamu apakah hari ke-putusan itu?",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 4,
				"end": 7
			},
			{
				"class": "qalqalah",
				"start": 9,
				"end": 10
			}
		]
	},
	{
		"aya_id": 5637,
		"aya_number": 15,
		"aya_text": "وَيْلٌ يَّوْمَىِٕذٍ لِّلْمُكَذِّبِيْنَ ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran).",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 4,
				"end": 9
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 17,
				"end": 22
			},
			{
				"class": "madd-arid-lissukun",
				"start": 34,
				"end": 36
			}
		]
	},
	{
		"aya_id": 5638,
		"aya_number": 16,
		"aya_text": "اَلَمْ نُهْلِكِ الْاَوَّلِيْنَۗ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "Bukankah telah Kami binasakan orang-orang yang dahulu?",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 26,
				"end": 28
			}
		]
	},
	{
		"aya_id": 5639,
		"aya_number": 17,
		"aya_text": "ثُمَّ نُتْبِعُهُمُ الْاٰخِرِيْنَ ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "Lalu Kami susulkan (azab Kami terhadap) orang-orang yang datang kemudian.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "madd-arid-lissukun",
				"start": 28,
				"end": 30
			}
		]
	},
	{
		"aya_id": 5640,
		"aya_number": 18,
		"aya_text": "كَذٰلِكَ نَفْعَلُ بِالْمُجْرِمِيْنَ ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "Demikianlah Kami perlakukan orang-orang yang berdosa.",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 25,
				"end": 26
			},
			{
				"class": "madd-arid-lissukun",
				"start": 31,
				"end": 33
			}
		]
	},
	{
		"aya_id": 5641,
		"aya_number": 19,
		"aya_text": "وَيْلٌ يَّوْمَىِٕذٍ لِّلْمُكَذِّبِيْنَ ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 580,
		"translation_aya_text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran).",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 4,
				"end": 9
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 17,
				"end": 22
			},
			{
				"class": "madd-arid-lissukun",
				"start": 34,
				"end": 36
			}
		]
	},
	{
		"aya_id": 5642,
		"aya_number": 20,
		"aya_text": "اَلَمْ نَخْلُقْكُّمْ مِّنْ مَّاۤءٍ مَّهِيْنٍۙ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "Bukankah Kami menciptakan kamu dari air yang hina (mani),",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 13,
				"end": 14
			},
			{
				"class": "idgham-mimi",
				"start": 18,
				"end": 23
			},
			{
				"class": "idgham-bighunnah",
				"start": 24,
				"end": 29
			},
			{
				"class": "madd-wajib",
				"start": 30,
				"end": 32
			},
			{
				"class": "idgham-bighunnah",
				"start": 32,
				"end": 37
			},
			{
				"class": "madd-arid-lissukun",
				"start": 40,
				"end": 42
			}
		]
	},
	{
		"aya_id": 5643,
		"aya_number": 21,
		"aya_text": "فَجَعَلْنٰهُ فِيْ قَرَارٍ مَّكِيْنٍ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "kemudian Kami letakkan ia dalam tempat yang kokoh (rahim),",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 23,
				"end": 28
			},
			{
				"class": "madd-arid-lissukun",
				"start": 31,
				"end": 33
			}
		]
	},
	{
		"aya_id": 5644,
		"aya_number": 22,
		"aya_text": "اِلٰى قَدَرٍ مَّعْلُوْمٍۙ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "sampai waktu yang ditentukan,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 10,
				"end": 15
			},
			{
				"class": "madd-arid-lissukun",
				"start": 20,
				"end": 22
			}
		]
	},
	{
		"aya_id": 5645,
		"aya_number": 23,
		"aya_text": "فَقَدَرْنَاۖ فَنِعْمَ الْقٰدِرُوْنَ ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "lalu Kami tentukan (bentuknya), maka (Kamilah) sebaik-baik yang menentukan.",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 31,
				"end": 33
			}
		]
	},
	{
		"aya_id": 5646,
		"aya_number": 24,
		"aya_text": "وَيْلٌ يَّوْمَىِٕذٍ لِّلْمُكَذِّبِيْنَ ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran).",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 4,
				"end": 9
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 17,
				"end": 22
			},
			{
				"class": "madd-arid-lissukun",
				"start": 34,
				"end": 36
			}
		]
	},
	{
		"aya_id": 5647,
		"aya_number": 25,
		"aya_text": "اَلَمْ نَجْعَلِ الْاَرْضَ كِفَاتًاۙ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "Bukankah Kami jadikan bumi untuk (tempat) berkumpul,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 9,
				"end": 10
			}
		]
	},
	{
		"aya_id": 5648,
		"aya_number": 26,
		"aya_text": "اَحْيَاۤءً وَّاَمْوَاتًاۙ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "bagi yang masih hidup dan yang sudah mati?",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 6,
				"end": 8
			},
			{
				"class": "idgham-bighunnah",
				"start": 8,
				"end": 13
			}
		]
	},
	{
		"aya_id": 5649,
		"aya_number": 27,
		"aya_text": "وَّجَعَلْنَا فِيْهَا رَوَاسِيَ شٰمِخٰتٍ وَّاَسْقَيْنٰكُمْ مَّاۤءً فُرَاتًاۗ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "Dan Kami jadikan padanya gunung-gunung yang tinggi, dan Kami beri minum kamu dengan air tawar?",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 37,
				"end": 42
			},
			{
				"class": "idgham-mimi",
				"start": 55,
				"end": 60
			},
			{
				"class": "madd-wajib",
				"start": 61,
				"end": 63
			},
			{
				"class": "ikhfa",
				"start": 63,
				"end": 68
			}
		]
	},
	{
		"aya_id": 5650,
		"aya_number": 28,
		"aya_text": "وَيْلٌ يَّوْمَىِٕذٍ لِّلْمُكَذِّبِيْنَ ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran).",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 4,
				"end": 9
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 17,
				"end": 22
			},
			{
				"class": "madd-arid-lissukun",
				"start": 34,
				"end": 36
			}
		]
	},
	{
		"aya_id": 5651,
		"aya_number": 29,
		"aya_text": "اِنْطَلِقُوْٓا اِلٰى مَا كُنْتُمْ بِهٖ تُكَذِّبُوْنَۚ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "(Akan dikatakan), “Pergilah kamu mendapatkan apa (azab) yang dahulu kamu dustakan.",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 2,
				"end": 6
			},
			{
				"class": "madd-jaiz",
				"start": 10,
				"end": 13
			},
			{
				"class": "ikhfa",
				"start": 27,
				"end": 31
			},
			{
				"class": "ikhfa-syafawi",
				"start": 31,
				"end": 36
			},
			{
				"class": "madd-arid-lissukun",
				"start": 48,
				"end": 50
			}
		]
	},
	{
		"aya_id": 5652,
		"aya_number": 30,
		"aya_text": "اِنْطَلِقُوْٓا اِلٰى ظِلٍّ ذِيْ ثَلٰثِ شُعَبٍ ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "Pergilah kamu mendapatkan naungan (asap api neraka) yang mempunyai tiga cabang, ",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 2,
				"end": 6
			},
			{
				"class": "madd-jaiz",
				"start": 10,
				"end": 13
			},
			{
				"class": "ikhfa",
				"start": 23,
				"end": 29
			}
		]
	},
	{
		"aya_id": 5653,
		"aya_number": 31,
		"aya_text": "لَا ظَلِيْلٍ وَّلَا يُغْنِيْ مِنَ اللَّهَبِۗ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "yang tidak melindungi dan tidak pula menolak nyala api neraka.” ",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 10,
				"end": 15
			}
		]
	},
	{
		"aya_id": 5654,
		"aya_number": 32,
		"aya_text": "اِنَّهَا تَرْمِيْ بِشَرَرٍ كَالْقَصْرِۚ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "Sungguh, (neraka) itu menyemburkan bunga api (sebesar dan setinggi) istana, ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "ikhfa",
				"start": 24,
				"end": 29
			}
		]
	},
	{
		"aya_id": 5655,
		"aya_number": 33,
		"aya_text": "كَاَنَّهٗ جِمٰلَتٌ صُفْرٌۗ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "seakan-akan iring-iringan unta yang kuning.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 6
			},
			{
				"class": "ikhfa",
				"start": 16,
				"end": 21
			}
		]
	},
	{
		"aya_id": 5656,
		"aya_number": 34,
		"aya_text": "وَيْلٌ يَّوْمَىِٕذٍ لِّلْمُكَذِّبِيْنَ ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran). ",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 4,
				"end": 9
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 17,
				"end": 22
			},
			{
				"class": "madd-arid-lissukun",
				"start": 34,
				"end": 36
			}
		]
	},
	{
		"aya_id": 5657,
		"aya_number": 35,
		"aya_text": "هٰذَا يَوْمُ لَا يَنْطِقُوْنَۙ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "Inilah hari, saat mereka tidak dapat berbicara, ",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 19,
				"end": 23
			},
			{
				"class": "madd-arid-lissukun",
				"start": 25,
				"end": 27
			}
		]
	},
	{
		"aya_id": 5658,
		"aya_number": 36,
		"aya_text": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُوْنَ ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "dan tidak diizinkan kepada mereka mengemukakan alasan agar mereka dimaafkan. ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 34,
				"end": 36
			}
		]
	},
	{
		"aya_id": 5659,
		"aya_number": 37,
		"aya_text": "وَيْلٌ يَّوْمَىِٕذٍ لِّلْمُكَذِّبِيْنَ ",
		"sura_id": 77,
		"juz_id": 29,
		"page_number": 581,
		"translation_aya_text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran). ",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 4,
				"end": 9
			},
			{
				"class": "idgham-bilaghunnah",
