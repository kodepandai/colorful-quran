[
	{
		"aya_id": 5324,
		"aya_number": 1,
		"aya_text": "اَلْحَاۤقَّةُۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 566,
		"translation_aya_text": "Hari Kiamat,",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 6,
				"end": 8
			}
		]
	},
	{
		"aya_id": 5325,
		"aya_number": 2,
		"aya_text": "مَا الْحَاۤقَّةُ ۚ ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 566,
		"translation_aya_text": "apakah hari Kiamat itu?",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 9,
				"end": 11
			}
		]
	},
	{
		"aya_id": 5326,
		"aya_number": 3,
		"aya_text": "وَمَآ اَدْرٰىكَ مَا الْحَاۤقَّةُ ۗ ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 566,
		"translation_aya_text": "Dan tahukah kamu apakah hari Kiamat itu?",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 3,
				"end": 7
			},
			{
				"class": "qalqalah",
				"start": 9,
				"end": 10
			},
			{
				"class": "madd-arid-lissukun",
				"start": 26,
				"end": 28
			}
		]
	},
	{
		"aya_id": 5327,
		"aya_number": 4,
		"aya_text": "كَذَّبَتْ ثَمُوْدُ وَعَادٌ ۢبِالْقَارِعَةِ ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 566,
		"translation_aya_text": "Kaum Samud, dan ‘Ad telah mendustakan hari Kiamat.",
		"tajweed": [
			{
				"class": "iqlab",
				"start": 24,
				"end": 26
			},
			{
				"class": "iqlab",
				"start": 27,
				"end": 29
			}
		]
	},
	{
		"aya_id": 5328,
		"aya_number": 5,
		"aya_text": "فَاَمَّا ثَمُوْدُ فَاُهْلِكُوْا بِالطَّاغِيَةِ ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 566,
		"translation_aya_text": "Maka adapun kaum Samud, mereka telah dibinasakan dengan suara yang sangat keras,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			}
		]
	},
	{
		"aya_id": 5329,
		"aya_number": 6,
		"aya_text": "وَاَمَّا عَادٌ فَاُهْلِكُوْا بِرِيْحٍ صَرْصَرٍ عَاتِيَةٍۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 566,
		"translation_aya_text": "sedangkan kaum ‘Ad, mereka telah dibinasakan dengan angin topan yang sangat dingin,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			},
			{
				"class": "ikhfa",
				"start": 12,
				"end": 17
			},
			{
				"class": "ikhfa",
				"start": 35,
				"end": 40
			}
		]
	},
	{
		"aya_id": 5330,
		"aya_number": 7,
		"aya_text": "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍ وَّثَمٰنِيَةَ اَيَّامٍۙ حُسُوْمًا فَتَرَى الْقَوْمَ فِيْهَا صَرْعٰىۙ  كَاَنَّهُمْ اَعْجَازُ نَخْلٍ خَاوِيَةٍۚ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 566,
		"translation_aya_text": "Allah menimpakan angin itu kepada mereka selama tujuh malam delapan hari terus-menerus; maka kamu melihat kaum ‘Ad pada waktu itu mati bergelimpangan seperti batang-batang pohon kurma yang telah kosong (lapuk).",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 24,
				"end": 25
			},
			{
				"class": "idgham-bighunnah",
				"start": 34,
				"end": 39
			},
			{
				"class": "ikhfa",
				"start": 67,
				"end": 69
			},
			{
				"class": "ikhfa",
				"start": 70,
				"end": 72
			},
			{
				"class": "ghunnah",
				"start": 111,
				"end": 114
			}
		]
	},
	{
		"aya_id": 5331,
		"aya_number": 8,
		"aya_text": "فَهَلْ تَرٰى لَهُمْ مِّنْۢ بَاقِيَةٍ ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 566,
		"translation_aya_text": "Maka adakah kamu melihat seorang pun yang masih tersisa di antara mereka?",
		"tajweed": [
			{
				"class": "idgham-mimi",
				"start": 17,
				"end": 22
			},
			{
				"class": "iqlab",
				"start": 23,
				"end": 25
			},
			{
				"class": "iqlab",
				"start": 26,
				"end": 28
			}
		]
	},
	{
		"aya_id": 5332,
		"aya_number": 9,
		"aya_text": "وَجَاۤءَ فِرْعَوْنُ وَمَنْ قَبْلَهٗ وَالْمُؤْتَفِكٰتُ بِالْخَاطِئَةِۚ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Kemudian datang Fir‘aun dan orang-orang yang sebelumnya dan (penduduk) negeri-negeri yang dijungkirbalikkan karena kesalahan yang besar.",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 3,
				"end": 6
			},
			{
				"class": "ikhfa",
				"start": 24,
				"end": 29
			},
			{
				"class": "qalqalah",
				"start": 29,
				"end": 30
			},
			{
				"class": "madd-arid-lissukun",
				"start": 61,
				"end": 62
			}
		]
	},
	{
		"aya_id": 5333,
		"aya_number": 10,
		"aya_text": "فَعَصَوْا رَسُوْلَ رَبِّهِمْ فَاَخَذَهُمْ اَخْذَةً رَّابِيَةً ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Maka mereka mendurhakai utusan Tuhannya, Allah menyiksa mereka dengan siksaan yang sangat keras. ",
		"tajweed": [
			{
				"class": "idgham-bilaghunnah",
				"start": 48,
				"end": 53
			}
		]
	},
	{
		"aya_id": 5334,
		"aya_number": 11,
		"aya_text": "اِنَّا لَمَّا طَغَا الْمَاۤءُ حَمَلْنٰكُمْ فِى الْجَارِيَةِۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Sesungguhnya ketika air naik (sampai ke gunung), Kami membawa (nenek moyang) kamu ke dalam kapal, ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			},
			{
				"class": "ghunnah",
				"start": 9,
				"end": 12
			},
			{
				"class": "madd-wajib",
				"start": 24,
				"end": 27
			}
		]
	},
	{
		"aya_id": 5335,
		"aya_number": 12,
		"aya_text": "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةً وَّتَعِيَهَآ اُذُنٌ وَّاعِيَةٌ ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "agar Kami jadikan (peristiwa itu) sebagai peringatan bagi kamu dan agar diperhatikan oleh telinga yang mau mendengar. ",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 4,
				"end": 5
			},
			{
				"class": "idgham-bighunnah",
				"start": 29,
				"end": 34
			},
			{
				"class": "madd-jaiz",
				"start": 42,
				"end": 46
			},
			{
				"class": "idgham-bighunnah",
				"start": 50,
				"end": 55
			}
		]
	},
	{
		"aya_id": 5336,
		"aya_number": 13,
		"aya_text": "فَاِذَا نُفِخَ فِى الصُّوْرِ نَفْخَةٌ وَّاحِدَةٌ ۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Maka apabila sangkakala ditiup sekali tiup,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 35,
				"end": 40
			}
		]
	},
	{
		"aya_id": 5337,
		"aya_number": 14,
		"aya_text": "وَّحُمِلَتِ الْاَرْضُ وَالْجِبَالُ فَدُكَّتَا دَكَّةً وَّاحِدَةًۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "dan diangkatlah bumi dan gunung-gunung, lalu dibenturkan keduanya sekali benturan.",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 51,
				"end": 56
			}
		]
	},
	{
		"aya_id": 5338,
		"aya_number": 15,
		"aya_text": "فَيَوْمَىِٕذٍ وَّقَعَتِ الْوَاقِعَةُۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Maka pada hari itu terjadilah hari Kiamat, ",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 11,
				"end": 16
			}
		]
	},
	{
		"aya_id": 5339,
		"aya_number": 16,
		"aya_text": "وَانْشَقَّتِ السَّمَاۤءُ فَهِيَ يَوْمَىِٕذٍ وَّاهِيَةٌۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "dan terbelahlah langit, karena pada hari itu langit menjadi rapuh.",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 3,
				"end": 7
			},
			{
				"class": "madd-wajib",
				"start": 19,
				"end": 22
			},
			{
				"class": "idgham-bighunnah",
				"start": 41,
				"end": 46
			}
		]
	},
	{
		"aya_id": 5340,
		"aya_number": 17,
		"aya_text": "وَّالْمَلَكُ عَلٰٓى اَرْجَاۤىِٕهَاۗ وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَىِٕذٍ ثَمٰنِيَةٌ  ۗ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Dan para malaikat berada di berbagai penjuru langit. Pada hari itu delapan malaikat menjunjung ‘Arsy (singgasana) Tuhanmu di atas (kepala) mereka. ",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 15,
				"end": 20
			},
			{
				"class": "madd-wajib",
				"start": 25,
				"end": 29
			},
			{
				"class": "ikhfa",
				"start": 82,
				"end": 87
			}
		]
	},
	{
		"aya_id": 5341,
		"aya_number": 18,
		"aya_text": "يَوْمَىِٕذٍ تُعْرَضُوْنَ لَا تَخْفٰى مِنْكُمْ خَافِيَةٌ ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Pada hari itu kamu dihadapkan (kepada Tuhanmu), tidak ada sesuatu pun dari kamu yang tersembunyi (bagi Allah). ",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 9,
				"end": 14
			},
			{
				"class": "ikhfa",
				"start": 39,
				"end": 43
			}
		]
	},
	{
		"aya_id": 5342,
		"aya_number": 19,
		"aya_text": "فَاَمَّا مَنْ اُوْتِيَ كِتٰبَهٗ بِيَمِيْنِهٖ فَيَقُوْلُ هَاۤؤُمُ اقْرَءُوْا كِتٰبِيَهْۚ ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Adapun orang yang kitabnya  diberikan di tangan kanannya, maka dia berkata, “Ambillah, bacalah kitabku (ini).” ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			},
			{
				"class": "qalqalah",
				"start": 66,
				"end": 67
			}
		]
	},
	{
		"aya_id": 5343,
		"aya_number": 20,
		"aya_text": "اِنِّيْ ظَنَنْتُ اَنِّيْ مُلٰقٍ حِسَابِيَهْۚ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Sesungguhnya aku yakin, bahwa (suatu saat) aku akan menerima perhitungan terhadap diriku. ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			},
			{
				"class": "ikhfa",
				"start": 12,
				"end": 16
			},
			{
				"class": "ghunnah",
				"start": 19,
				"end": 22
			}
		]
	},
	{
		"aya_id": 5344,
		"aya_number": 21,
		"aya_text": "فَهُوَ فِيْ عِيْشَةٍ رَّاضِيَةٍۚ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Maka orang itu berada dalam kehidupan yang diridai,",
		"tajweed": [
			{
				"class": "idgham-bilaghunnah",
				"start": 18,
				"end": 23
			}
		]
	},
	{
		"aya_id": 5345,
		"aya_number": 22,
		"aya_text": "فِيْ جَنَّةٍ عَالِيَةٍۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "dalam surga yang tinggi, ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 7,
				"end": 10
			}
		]
	},
	{
		"aya_id": 5346,
		"aya_number": 23,
		"aya_text": "قُطُوْفُهَا دَانِيَةٌ ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "buah-buahannya dekat,",
		"tajweed": []
	},
	{
		"aya_id": 5347,
		"aya_number": 24,
		"aya_text": "كُلُوْا وَاشْرَبُوْا هَنِيْۤـًٔا ۢبِمَآ اَسْلَفْتُمْ فِى الْاَيَّامِ الْخَالِيَةِ ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "(kepada mereka dikatakan), “Makan dan minumlah dengan nikmat karena amal yang telah kamu kerjakan pada hari-hari yang telah lalu.” ",
		"tajweed": [
			{
				"class": "iqlab",
				"start": 28,
				"end": 31
			},
			{
				"class": "iqlab",
				"start": 33,
				"end": 35
			},
			{
				"class": "madd-jaiz",
				"start": 37,
				"end": 41
			}
		]
	},
	{
		"aya_id": 5348,
		"aya_number": 25,
		"aya_text": "وَاَمَّا مَنْ اُوْتِيَ كِتٰبَهٗ بِشِمَالِهٖ ەۙ فَيَقُوْلُ يٰلَيْتَنِيْ لَمْ اُوْتَ كِتٰبِيَهْۚ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Dan adapun orang yang kitabnya diberikan di tangan kirinya, maka dia berkata, “Alangkah baiknya jika kitabku (ini) tidak diberikan kepadaku.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			}
		]
	},
	{
		"aya_id": 5349,
		"aya_number": 26,
		"aya_text": "وَلَمْ اَدْرِ مَا حِسَابِيَهْۚ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Sehingga aku tidak mengetahui bagaimana perhitunganku. ",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 9,
				"end": 10
			}
		]
	},
	{
		"aya_id": 5350,
		"aya_number": 27,
		"aya_text": "يٰلَيْتَهَا كَانَتِ الْقَاضِيَةَۚ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Wahai, kiranya (kematian) itulah yang menyudahi segala sesuatu. ",
		"tajweed": []
	},
	{
		"aya_id": 5351,
		"aya_number": 28,
		"aya_text": "مَآ اَغْنٰى عَنِّيْ مَالِيَهْۚ ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Hartaku sama sekali tidak berguna bagiku. ",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 1,
				"end": 5
			},
			{
				"class": "ghunnah",
				"start": 15,
				"end": 18
			}
		]
	},
	{
		"aya_id": 5352,
		"aya_number": 29,
		"aya_text": "هَلَكَ عَنِّيْ سُلْطٰنِيَهْۚ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Kekuasaanku telah hilang dariku.”",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 9,
				"end": 12
			}
		]
	},
	{
		"aya_id": 5353,
		"aya_number": 30,
		"aya_text": "خُذُوْهُ فَغُلُّوْهُۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "(Allah berfirman), “Tangkaplah dia lalu belenggulah tangannya ke lehernya.” ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 16,
				"end": 18
			}
		]
	},
	{
		"aya_id": 5354,
		"aya_number": 31,
		"aya_text": "ثُمَّ الْجَحِيْمَ صَلُّوْهُۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Kemudian masukkanlah dia ke dalam api neraka yang menyala-nyala. ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			},
			{
				"class": "madd-arid-lissukun",
				"start": 23,
				"end": 25
			}
		]
	},
	{
		"aya_id": 5355,
		"aya_number": 32,
		"aya_text": "ثُمَّ فِيْ سِلْسِلَةٍ ذَرْعُهَا سَبْعُوْنَ ذِرَاعًا فَاسْلُكُوْهُۗ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Kemudian belitlah dia dengan rantai yang panjangnya tujuh puluh hasta. ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			},
			{
				"class": "ikhfa",
				"start": 19,
				"end": 24
			},
			{
				"class": "qalqalah",
				"start": 34,
				"end": 35
			},
			{
				"class": "ikhfa",
				"start": 48,
				"end": 50
			},
			{
				"class": "ikhfa",
				"start": 51,
				"end": 53
			},
			{
				"class": "madd-arid-lissukun",
				"start": 61,
				"end": 63
			}
		]
	},
	{
		"aya_id": 5356,
		"aya_number": 33,
		"aya_text": "اِنَّهٗ كَانَ لَا يُؤْمِنُ بِاللّٰهِ الْعَظِيْمِۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Sesungguhnya dialah orang yang tidak beriman kepada Allah Yang Mahabesar. ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			},
			{
				"class": "madd-arid-lissukun",
				"start": 44,
				"end": 46
			}
		]
	},
	{
		"aya_id": 5357,
		"aya_number": 34,
		"aya_text": "وَلَا يَحُضُّ عَلٰى طَعَامِ الْمِسْكِيْنِۗ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 567,
		"translation_aya_text": "Dan juga dia tidak mendorong (orang lain) untuk memberi makan orang miskin. ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 37,
				"end": 39
			}
		]
	},
	{
		"aya_id": 5358,
		"aya_number": 35,
		"aya_text": "فَلَيْسَ لَهُ الْيَوْمَ هٰهُنَا حَمِيْمٌۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Maka pada hari ini di sini tidak ada seorang teman pun baginya. ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 36,
				"end": 38
			}
		]
	},
	{
		"aya_id": 5359,
		"aya_number": 36,
		"aya_text": "وَّلَا طَعَامٌ اِلَّا مِنْ غِسْلِيْنٍۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Dan tidak ada makanan (baginya) kecuali dari darah dan nanah. ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 33,
				"end": 35
			}
		]
	},
	{
		"aya_id": 5360,
		"aya_number": 37,
		"aya_text": "لَّا يَأْكُلُهٗٓ اِلَّا الْخَاطِـُٔوْنَ ࣖ ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Tidak ada yang memakannya kecuali orang-orang yang berdosa. ",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 13,
				"end": 17
			},
			{
				"class": "madd-arid-lissukun",
				"start": 35,
				"end": 37
			}
		]
	},
	{
		"aya_id": 5361,
		"aya_number": 38,
		"aya_text": "فَلَآ اُقْسِمُ بِمَا تُبْصِرُوْنَۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Maka Aku bersumpah demi apa yang kamu lihat, ",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 3,
				"end": 7
			},
			{
				"class": "qalqalah",
				"start": 9,
				"end": 10
			},
			{
				"class": "qalqalah",
				"start": 24,
				"end": 25
			},
			{
				"class": "madd-arid-lissukun",
				"start": 30,
				"end": 32
			}
		]
	},
	{
		"aya_id": 5362,
		"aya_number": 39,
		"aya_text": "وَمَا لَا تُبْصِرُوْنَۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "dan demi apa yang tidak kamu lihat.",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 12,
				"end": 13
			},
			{
				"class": "madd-arid-lissukun",
				"start": 18,
				"end": 20
			}
		]
	},
	{
		"aya_id": 5363,
		"aya_number": 40,
		"aya_text": "اِنَّهٗ لَقَوْلُ رَسُوْلٍ كَرِيْمٍۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Sesungguhnya ia (Al-Qur'an) itu benar-benar wahyu (yang diturunkan kepada) Rasul yang mulia,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			},
			{
				"class": "ikhfa",
				"start": 23,
				"end": 28
			},
			{
				"class": "madd-arid-lissukun",
				"start": 30,
				"end": 32
			}
		]
	},
	{
		"aya_id": 5364,
		"aya_number": 41,
		"aya_text": "وَّمَا هُوَ بِقَوْلِ شَاعِرٍۗ قَلِيْلًا مَّا تُؤْمِنُوْنَۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "dan ia (Al-Qur'an) bukanlah perkataan seorang penyair. Sedikit sekali kamu beriman kepadanya. ",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 26,
				"end": 28
			},
			{
				"class": "ikhfa",
				"start": 29,
				"end": 31
			},
			{
				"class": "idgham-bighunnah",
				"start": 36,
				"end": 38
			},
			{
				"class": "idgham-bighunnah",
				"start": 39,
				"end": 41
			},
			{
				"class": "madd-arid-lissukun",
				"start": 53,
				"end": 55
			}
		]
	},
	{
		"aya_id": 5365,
		"aya_number": 42,
		"aya_text": "وَلَا بِقَوْلِ كَاهِنٍۗ قَلِيْلًا مَّا تَذَكَّرُوْنَۗ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Dan bukan pula perkataan tukang tenung. Sedikit sekali kamu mengambil pelajaran darinya. ",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 20,
				"end": 22
			},
			{
				"class": "ikhfa",
				"start": 23,
				"end": 25
			},
			{
				"class": "idgham-bighunnah",
				"start": 30,
				"end": 32
			},
			{
				"class": "idgham-bighunnah",
				"start": 33,
				"end": 35
			},
			{
				"class": "madd-arid-lissukun",
				"start": 48,
				"end": 50
			}
		]
	},
	{
		"aya_id": 5366,
		"aya_number": 43,
		"aya_text": "تَنْزِيْلٌ مِّنْ رَّبِّ الْعٰلَمِيْنَ ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Ia (Al-Qur'an) adalah wahyu yang diturunkan dari Tuhan seluruh alam. ",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 2,
				"end": 6
			},
			{
				"class": "idgham-bighunnah",
				"start": 8,
				"end": 13
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 14,
				"end": 19
			},
			{
				"class": "madd-arid-lissukun",
				"start": 33,
				"end": 35
			}
		]
	},
	{
		"aya_id": 5367,
		"aya_number": 44,
		"aya_text": "وَلَوْ تَقَوَّلَ عَلَيْنَا بَعْضَ الْاَقَاوِيْلِۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Dan sekiranya dia (Muhammad) mengada-adakan sebagian perkataan atas (nama) Kami, ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 44,
				"end": 46
			}
		]
	},
	{
		"aya_id": 5368,
		"aya_number": 45,
		"aya_text": "لَاَخَذْنَا مِنْهُ بِالْيَمِيْنِۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "pasti Kami pegang dia pada tangan kanannya. ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 28,
				"end": 30
			}
		]
	},
	{
		"aya_id": 5369,
		"aya_number": 46,
		"aya_text": "ثُمَّ لَقَطَعْنَا مِنْهُ الْوَتِيْنَۖ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Kemudian Kami potong pembuluh jantungnya. ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			},
			{
				"class": "madd-arid-lissukun",
				"start": 32,
				"end": 34
			}
		]
	},
	{
		"aya_id": 5370,
		"aya_number": 47,
		"aya_text": "فَمَا مِنْكُمْ مِّنْ اَحَدٍ عَنْهُ حَاجِزِيْنَۙ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Maka tidak seorang pun dari kamu yang dapat menghalangi (Kami untuk menghukumnya). ",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 8,
				"end": 12
			},
			{
				"class": "idgham-mimi",
				"start": 12,
				"end": 17
			},
			{
				"class": "madd-arid-lissukun",
				"start": 42,
				"end": 44
			}
		]
	},
	{
		"aya_id": 5371,
		"aya_number": 48,
		"aya_text": "وَاِنَّهٗ لَتَذْكِرَةٌ لِّلْمُتَّقِيْنَ ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Dan sungguh, (Al-Qur'an) itu pelajaran bagi orang-orang yang bertakwa. ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 20,
				"end": 25
			},
			{
				"class": "madd-arid-lissukun",
				"start": 35,
				"end": 37
			}
		]
	},
	{
		"aya_id": 5372,
		"aya_number": 49,
		"aya_text": "وَاِنَّا لَنَعْلَمُ اَنَّ مِنْكُمْ مُّكَذِّبِيْنَۗ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Dan sungguh, Kami mengetahui bahwa di antara kamu ada orang yang mendustakan.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			},
			{
				"class": "ghunnah",
				"start": 22,
				"end": 25
			},
			{
				"class": "ikhfa",
				"start": 28,
				"end": 32
			},
			{
				"class": "idgham-mimi",
				"start": 32,
				"end": 37
			},
			{
				"class": "madd-arid-lissukun",
				"start": 45,
				"end": 47
			}
		]
	},
	{
		"aya_id": 5373,
		"aya_number": 50,
		"aya_text": "وَاِنَّهٗ لَحَسْرَةٌ عَلَى الْكٰفِرِيْنَۚ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Dan sungguh, (Al-Qur'an) itu akan menimbulkan penyesalan bagi orang-orang kafir (di akhirat).",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			},
			{
				"class": "madd-arid-lissukun",
				"start": 36,
				"end": 38
			}
		]
	},
	{
		"aya_id": 5374,
		"aya_number": 51,
		"aya_text": "وَاِنَّهٗ لَحَقُّ الْيَقِيْنِ ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Dan Sungguh, (Al-Qur'an) itu kebenaran yang meyakinkan. ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			},
			{
				"class": "madd-arid-lissukun",
				"start": 25,
				"end": 27
			}
		]
	},
	{
		"aya_id": 5375,
		"aya_number": 52,
		"aya_text": "فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِيْمِ ࣖ",
		"sura_id": 69,
		"juz_id": 29,
		"page_number": 568,
		"translation_aya_text": "Maka bertasbihlah dengan (menyebut) nama Tuhanmu Yang Mahaagung. ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 33,
				"end": 35
			}
		]
	}
]
