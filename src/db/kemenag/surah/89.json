[
	{
		"aya_id": 5994,
		"aya_number": 1,
		"aya_text": "وَالْفَجْرِۙ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "Demi fajar,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 7,
				"end": 8
			}
		]
	},
	{
		"aya_id": 5995,
		"aya_number": 2,
		"aya_text": "وَلَيَالٍ عَشْرٍۙ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "demi malam yang sepuluh,",
		"tajweed": []
	},
	{
		"aya_id": 5996,
		"aya_number": 3,
		"aya_text": "وَّالشَّفْعِ وَالْوَتْرِۙ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "demi yang genap dan yang ganjil,",
		"tajweed": []
	},
	{
		"aya_id": 5997,
		"aya_number": 4,
		"aya_text": "وَالَّيْلِ اِذَا يَسْرِۚ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "demi malam apabila berlalu.",
		"tajweed": []
	},
	{
		"aya_id": 5998,
		"aya_number": 5,
		"aya_text": "هَلْ فِيْ ذٰلِكَ قَسَمٌ لِّذِيْ حِجْرٍۗ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "Adakah pada yang demikian itu terdapat sumpah (yang dapat diterima) bagi orang-orang yang berakal?",
		"tajweed": [
			{
				"class": "idgham-bilaghunnah",
				"start": 21,
				"end": 26
			},
			{
				"class": "qalqalah",
				"start": 34,
				"end": 35
			}
		]
	},
	{
		"aya_id": 5999,
		"aya_number": 6,
		"aya_text": "اَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍۖ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "Tidakkah engkau (Muhammad) memperhatikan bagaimana Tuhanmu berbuat terhadap (kaum) ‘Ad?",
		"tajweed": []
	},
	{
		"aya_id": 6000,
		"aya_number": 7,
		"aya_text": "اِرَمَ ذَاتِ الْعِمَادِۖ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "(yaitu) penduduk Iram (ibukota kaum ‘Ad) yang mempunyai bangunan-bangunan yang tinggi,",
		"tajweed": []
	},
	{
		"aya_id": 6001,
		"aya_number": 8,
		"aya_text": "الَّتِيْ لَمْ يُخْلَقْ مِثْلُهَا فِى الْبِلَادِۖ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "yang belum pernah dibangun (suatu kota) seperti itu di negeri-negeri lain,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 20,
				"end": 21
			}
		]
	},
	{
		"aya_id": 6002,
		"aya_number": 9,
		"aya_text": "وَثَمُوْدَ الَّذِيْنَ جَابُوا الصَّخْرَ بِالْوَادِۖ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "dan (terhadap) kaum samud yang memotong batu-batu besar di lembah,",
		"tajweed": []
	},
	{
		"aya_id": 6003,
		"aya_number": 10,
		"aya_text": "وَفِرْعَوْنَ ذِى الْاَوْتَادِۖ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "dan (terhadap) Fir‘aun yang mempunyai pasak-pasak (bangunan yang besar),",
		"tajweed": []
	},
	{
		"aya_id": 6004,
		"aya_number": 11,
		"aya_text": "الَّذِيْنَ طَغَوْا فِى الْبِلَادِۖ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "yang berbuat sewenang-wenang dalam negeri,",
		"tajweed": []
	},
	{
		"aya_id": 6005,
		"aya_number": 12,
		"aya_text": "فَاَكْثَرُوْا فِيْهَا الْفَسَادَۖ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "lalu mereka banyak berbuat kerusakan dalam negeri itu,",
		"tajweed": []
	},
	{
		"aya_id": 6006,
		"aya_number": 13,
		"aya_text": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍۖ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "karena itu Tuhanmu menimpakan cemeti azab kepada mereka,",
		"tajweed": []
	},
	{
		"aya_id": 6007,
		"aya_number": 14,
		"aya_text": "اِنَّ رَبَّكَ لَبِالْمِرْصَادِۗ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "sungguh, Tuhanmu benar-benar mengawasi.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			}
		]
	},
	{
		"aya_id": 6008,
		"aya_number": 15,
		"aya_text": "فَاَمَّا الْاِنْسَانُ اِذَا مَا ابْتَلٰىهُ رَبُّهٗ فَاَكْرَمَهٗ وَنَعَّمَهٗۙ فَيَقُوْلُ رَبِّيْٓ اَكْرَمَنِۗ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "Maka adapun manusia, apabila Tuhan mengujinya lalu memuliakannya dan memberinya kesenangan, maka dia berkata, “Tuhanku telah memuliakanku.”",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			},
			{
				"class": "ikhfa",
				"start": 14,
				"end": 18
			},
			{
				"class": "qalqalah",
				"start": 33,
				"end": 34
			},
			{
				"class": "madd-jaiz",
				"start": 93,
				"end": 97
			}
		]
	},
	{
		"aya_id": 6009,
		"aya_number": 16,
		"aya_text": "وَاَمَّآ اِذَا مَا ابْتَلٰىهُ فَقَدَرَ عَلَيْهِ رِزْقَهٗ ەۙ فَيَقُوْلُ رَبِّيْٓ اَهَانَنِۚ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "Namun apabila Tuhan mengujinya lalu membatasi rezekinya, maka dia berkata, “Tuhanku telah menghinaku.”",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			},
			{
				"class": "madd-jaiz",
				"start": 6,
				"end": 10
			},
			{
				"class": "qalqalah",
				"start": 21,
				"end": 22
			},
			{
				"class": "madd-jaiz",
				"start": 77,
				"end": 81
			}
		]
	},
	{
		"aya_id": 6010,
		"aya_number": 17,
		"aya_text": "كَلَّا بَلْ لَّا تُكْرِمُوْنَ الْيَتِيْمَۙ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "Sekali-kali tidak! Bahkan kamu tidak memuliakan anak yatim,",
		"tajweed": []
	},
	{
		"aya_id": 6011,
		"aya_number": 18,
		"aya_text": "وَلَا تَحٰۤضُّوْنَ عَلٰى طَعَامِ الْمِسْكِيْنِۙ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "dan kamu tidak saling mengajak memberi makan orang miskin,",
		"tajweed": []
	},
	{
		"aya_id": 6012,
		"aya_number": 19,
		"aya_text": "وَتَأْكُلُوْنَ التُّرَاثَ اَكْلًا لَّمًّاۙ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "sedangkan kamu memakan harta warisan dengan cara mencampurbaurkan (yang halal dan yang haram),",
		"tajweed": [
			{
				"class": "idgham-bilaghunnah",
				"start": 30,
				"end": 32
			},
			{
				"class": "idgham-bilaghunnah",
				"start": 33,
				"end": 35
			},
			{
				"class": "ghunnah",
				"start": 37,
				"end": 40
			}
		]
	},
	{
		"aya_id": 6013,
		"aya_number": 20,
		"aya_text": "وَّتُحِبُّوْنَ الْمَالَ حُبًّا جَمًّاۗ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "dan kamu mencintai harta dengan kecintaan yang berlebihan.",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 26,
				"end": 29
			},
			{
				"class": "ikhfa",
				"start": 30,
				"end": 32
			},
			{
				"class": "ghunnah",
				"start": 33,
				"end": 36
			}
		]
	},
	{
		"aya_id": 6014,
		"aya_number": 21,
		"aya_text": "كَلَّآ اِذَا دُكَّتِ الْاَرْضُ دَكًّا دَكًّاۙ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "Sekali-kali tidak! Apabila bumi diguncangkan berturut-turut (berbenturan),",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 4,
				"end": 8
			},
			{
				"class": "ikhfa",
				"start": 34,
				"end": 37
			},
			{
				"class": "ikhfa",
				"start": 38,
				"end": 40
			}
		]
	},
	{
		"aya_id": 6015,
		"aya_number": 22,
		"aya_text": "وَّجَآءَ رَبُّكَ وَالْمَلَكُ صَفًّا صَفًّاۚ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "dan datanglah Tuhanmu; dan malaikat berbaris-baris,",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 4,
				"end": 7
			},
			{
				"class": "ikhfa",
				"start": 32,
				"end": 35
			},
			{
				"class": "ikhfa",
				"start": 36,
				"end": 38
			}
		]
	},
	{
		"aya_id": 6016,
		"aya_number": 23,
		"aya_text": "وَجِايْۤءَ يَوْمَىِٕذٍۢ بِجَهَنَّمَۙ يَوْمَىِٕذٍ يَّتَذَكَّرُ الْاِنْسَانُ وَاَنّٰى لَهُ الذِّكْرٰىۗ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 593,
		"translation_aya_text": "dan pada hari itu diperlihatkan neraka Jahanam; pada hari itu sadarlah manusia, tetapi tidak berguna lagi baginya kesadaran itu.",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 5,
				"end": 8
			},
			{
				"class": "iqlab",
				"start": 20,
				"end": 22
			},
			{
				"class": "iqlab",
				"start": 23,
				"end": 25
			},
			{
				"class": "ghunnah",
				"start": 30,
				"end": 33
			},
			{
				"class": "idgham-bighunnah",
				"start": 46,
				"end": 51
			},
			{
				"class": "ikhfa",
				"start": 67,
				"end": 71
			},
			{
				"class": "ghunnah",
				"start": 79,
				"end": 82
			}
		]
	},
	{
		"aya_id": 6017,
		"aya_number": 24,
		"aya_text": "يَقُوْلُ يٰلَيْتَنِيْ قَدَّمْتُ لِحَيَاتِيْۚ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 594,
		"translation_aya_text": "Dia berkata, “Alangkah baiknya sekiranya dahulu aku mengerjakan (kebajikan) untuk hidupku ini.”",
		"tajweed": []
	},
	{
		"aya_id": 6018,
		"aya_number": 25,
		"aya_text": "فَيَوْمَىِٕذٍ لَّا يُعَذِّبُ عَذَابَهٗٓ اَحَدٌ ۙ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 594,
		"translation_aya_text": "Maka pada hari itu tidak ada seorang pun yang mengazab seperti azab-Nya (yang adil),",
		"tajweed": [
			{
				"class": "idgham-bilaghunnah",
				"start": 11,
				"end": 16
			},
			{
				"class": "madd-jaiz",
				"start": 36,
				"end": 40
			}
		]
	},
	{
		"aya_id": 6019,
		"aya_number": 26,
		"aya_text": "وَّلَا يُوْثِقُ وَثَاقَهٗٓ اَحَدٌ ۗ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 594,
		"translation_aya_text": "dan tidak ada seorang pun yang mengikat seperti ikatan-Nya.",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 23,
				"end": 27
			}
		]
	},
	{
		"aya_id": 6020,
		"aya_number": 27,
		"aya_text": "يٰٓاَيَّتُهَا النَّفْسُ الْمُطْمَىِٕنَّةُۙ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 594,
		"translation_aya_text": "Wahai jiwa yang tenang!",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 0,
				"end": 3
			},
			{
				"class": "ghunnah",
				"start": 16,
				"end": 19
			},
			{
				"class": "qalqalah",
				"start": 29,
				"end": 30
			},
			{
				"class": "ghunnah",
				"start": 36,
				"end": 39
			}
		]
	},
	{
		"aya_id": 6021,
		"aya_number": 28,
		"aya_text": "ارْجِعِيْٓ اِلٰى رَبِّكِ رَاضِيَةً مَّرْضِيَّةً ۚ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 594,
		"translation_aya_text": "Kembalilah kepada Tuhanmu dengan hati yang rida dan diridai-Nya.",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 7,
				"end": 11
			},
			{
				"class": "idgham-bighunnah",
				"start": 32,
				"end": 37
			}
		]
	},
	{
		"aya_id": 6022,
		"aya_number": 29,
		"aya_text": "فَادْخُلِيْ فِيْ عِبٰدِيْۙ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 594,
		"translation_aya_text": "Maka masuklah ke dalam golongan hamba-hamba-Ku,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 3,
				"end": 4
			}
		]
	},
	{
		"aya_id": 6023,
		"aya_number": 30,
		"aya_text": "وَادْخُلِيْ جَنَّتِيْ ࣖࣖ",
		"sura_id": 89,
		"juz_id": 30,
		"page_number": 594,
		"translation_aya_text": "dan masuklah ke dalam surga-Ku.",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 3,
				"end": 4
			},
			{
				"class": "ghunnah",
				"start": 14,
				"end": 17
			}
		]
	}
]
