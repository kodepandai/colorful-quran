[
	{
		"aya_id": 4902,
		"aya_number": 1,
		"aya_text": "اَلرَّحْمٰنُۙ  ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 531,
		"translation_aya_text": "(Allah) Yang Maha Pengasih,",
		"tajweed": []
	},
	{
		"aya_id": 4903,
		"aya_number": 2,
		"aya_text": "عَلَّمَ الْقُرْاٰنَۗ ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 531,
		"translation_aya_text": "Yang telah mengajarkan Al-Qur'an.",
		"tajweed": []
	},
	{
		"aya_id": 4904,
		"aya_number": 3,
		"aya_text": "خَلَقَ الْاِنْسَانَۙ ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 531,
		"translation_aya_text": "Dia menciptakan manusia, ",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 12,
				"end": 16
			},
			{
				"class": "madd-arid-lissukun",
				"start": 16,
				"end": 17
			}
		]
	},
	{
		"aya_id": 4905,
		"aya_number": 4,
		"aya_text": "عَلَّمَهُ الْبَيَانَ ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 531,
		"translation_aya_text": "mengajarnya pandai berbicara.",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 17,
				"end": 18
			}
		]
	},
	{
		"aya_id": 4906,
		"aya_number": 5,
		"aya_text": "اَلشَّمْسُ وَالْقَمَرُ بِحُسْبَانٍۙ ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 531,
		"translation_aya_text": "Matahari dan bulan beredar menurut perhitungan,",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 31,
				"end": 32
			}
		]
	},
	{
		"aya_id": 4907,
		"aya_number": 6,
		"aya_text": "وَّالنَّجْمُ وَالشَّجَرُ يَسْجُدَانِ ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 531,
		"translation_aya_text": "dan tetumbuhan dan pepohonan, keduanya tunduk (kepada-Nya). ",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 5,
				"end": 7
			},
			{
				"class": "qalqalah",
				"start": 8,
				"end": 9
			},
			{
				"class": "madd-arid-lissukun",
				"start": 33,
				"end": 34
			}
		]
	},
	{
		"aya_id": 4908,
		"aya_number": 7,
		"aya_text": "وَالسَّمَاۤءَ رَفَعَهَا وَوَضَعَ الْمِيْزَانَۙ  ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 531,
		"translation_aya_text": "Dan langit telah ditinggikan-Nya dan Dia ciptakan keseimbangan,",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 9,
				"end": 11
			},
			{
				"class": "madd-arid-lissukun",
				"start": 42,
				"end": 43
			}
		]
	},
	{
		"aya_id": 4909,
		"aya_number": 8,
		"aya_text": "اَلَّا تَطْغَوْا فِى الْمِيْزَانِ ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 531,
		"translation_aya_text": "agar kamu jangan merusak keseimbangan itu, ",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 9,
				"end": 10
			},
			{
				"class": "madd-arid-lissukun",
				"start": 30,
				"end": 31
			}
		]
	},
	{
		"aya_id": 4910,
		"aya_number": 9,
		"aya_text": "وَاَقِيْمُوا الْوَزْنَ بِالْقِسْطِ وَلَا تُخْسِرُوا الْمِيْزَانَ ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 531,
		"translation_aya_text": "dan tegakkanlah keseimbangan itu dengan adil dan janganlah kamu mengurangi keseimbangan itu. ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 61,
				"end": 62
			}
		]
	},
	{
		"aya_id": 4911,
		"aya_number": 10,
		"aya_text": "وَالْاَرْضَ وَضَعَهَا لِلْاَنَامِۙ  ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 531,
		"translation_aya_text": "Dan bumi telah dibentangkan-Nya untuk makhluk(-Nya), ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 30,
				"end": 31
			}
		]
	},
	{
		"aya_id": 4912,
		"aya_number": 11,
		"aya_text": "فِيْهَا فَاكِهَةٌ وَّالنَّخْلُ ذَاتُ الْاَكْمَامِۖ  ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 531,
		"translation_aya_text": "di dalamnya ada buah-buahan dan pohon kurma yang mempunyai kelopak mayang, ",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 15,
				"end": 20
			},
			{
				"class": "ghunnah",
				"start": 23,
				"end": 25
			},
			{
				"class": "madd-arid-lissukun",
				"start": 46,
				"end": 47
			}
		]
	},
	{
		"aya_id": 4913,
		"aya_number": 12,
		"aya_text": "وَالْحَبُّ ذُو الْعَصْفِ وَالرَّيْحَانُۚ  ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 531,
		"translation_aya_text": "dan biji-bijian yang berkulit dan bunga-bunga yang harum baunya. ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 36,
				"end": 37
			}
		]
	},
	{
		"aya_id": 4914,
		"aya_number": 13,
		"aya_text": "فَبِاَيِّ اٰلَاۤءِ رَبِّكُمَا تُكَذِّبٰنِ ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 531,
		"translation_aya_text": "Maka nikmat Tuhanmu yang manakah yang kamu dustakan? ",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 14,
				"end": 16
			}
		]
	},
	{
		"aya_id": 4915,
		"aya_number": 14,
		"aya_text": "خَلَقَ الْاِنْسَانَ مِنْ صَلْصَالٍ كَالْفَخَّارِ ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 531,
		"translation_aya_text": "Dia menciptakan manusia dari tanah kering seperti tembikar,",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 12,
				"end": 16
			},
			{
				"class": "ikhfa",
				"start": 22,
				"end": 27
			},
			{
				"class": "ikhfa",
				"start": 32,
				"end": 37
			},
			{
				"class": "madd-arid-lissukun",
				"start": 45,
				"end": 46
			}
		]
	},
	{
		"aya_id": 4916,
		"aya_number": 15,
		"aya_text": "وَخَلَقَ الْجَاۤنَّ مِنْ مَّارِجٍ مِّنْ نَّارٍۚ  ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 531,
		"translation_aya_text": "dan Dia menciptakan jin dari nyala api tanpa asap.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 16,
				"end": 18
			},
			{
				"class": "idgham-bighunnah",
				"start": 22,
				"end": 27
			},
			{
				"class": "idgham-bighunnah",
				"start": 31,
				"end": 36
			},
			{
