	},
	{
		"aya_id": 4929,
		"aya_number": 28,
		"aya_text": "فَبِاَيِّ اٰلَاۤءِ رَبِّكُمَا تُكَذِّبٰنِ ",
		"sura_id": 55,
		"juz_id": 27,
		"page_number": 532,
