[
	{
		"aya_id": 6044,
		"aya_number": 1,
		"aya_text": "وَالشَّمْسِ وَضُحٰىهَاۖ",
		"sura_id": 91,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "Demi matahari dan sinarnya pada pagi hari,"
	},
	{
		"aya_id": 6045,
		"aya_number": 2,
		"aya_text": "وَالْقَمَرِ اِذَا تَلٰىهَاۖ",
		"sura_id": 91,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "demi bulan apabila mengiringinya,"
	},
	{
		"aya_id": 6046,
		"aya_number": 3,
		"aya_text": "وَالنَّهَارِ اِذَا جَلّٰىهَاۖ",
		"sura_id": 91,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "demi siang apabila menampakkannya,"
	},
	{
		"aya_id": 6047,
		"aya_number": 4,
		"aya_text": "وَالَّيْلِ اِذَا يَغْشٰىهَاۖ",
		"sura_id": 91,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "demi malam apabila menutupinya (gelap gulita),"
	},
	{
		"aya_id": 6048,
		"aya_number": 5,
		"aya_text": "وَالسَّمَاۤءِ وَمَا بَنٰىهَاۖ",
		"sura_id": 91,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "demi langit serta pembinaannya (yang menakjubkan),"
	},
	{
		"aya_id": 6049,
		"aya_number": 6,
		"aya_text": "وَالْاَرْضِ وَمَا طَحٰىهَاۖ",
		"sura_id": 91,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "demi bumi serta penghamparannya,"
	},
	{
		"aya_id": 6050,
		"aya_number": 7,
		"aya_text": "وَنَفْسٍ وَّمَا سَوّٰىهَاۖ",
		"sura_id": 91,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "demi jiwa serta penyempurnaan (ciptaan)nya,"
	},
	{
		"aya_id": 6051,
		"aya_number": 8,
		"aya_text": "فَاَلْهَمَهَا فُجُوْرَهَا وَتَقْوٰىهَاۖ",
		"sura_id": 91,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "maka Dia mengilhamkan kepadanya (jalan) kejahatan dan ketakwaannya,"
	},
	{
		"aya_id": 6052,
		"aya_number": 9,
		"aya_text": "قَدْ اَفْلَحَ مَنْ زَكّٰىهَاۖ",
		"sura_id": 91,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "sungguh beruntung orang yang menyucikannya (jiwa itu),"
	},
	{
		"aya_id": 6053,
		"aya_number": 10,
		"aya_text": "وَقَدْ خَابَ مَنْ دَسّٰىهَاۗ",
		"sura_id": 91,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "dan sungguh rugi orang yang mengotorinya."
	},
	{
		"aya_id": 6054,
		"aya_number": 11,
		"aya_text": "كَذَّبَتْ ثَمُوْدُ بِطَغْوٰىهَآ  ۖ",
		"sura_id": 91,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "(Kaum) samud telah mendustakan (rasulnya) karena mereka melampaui batas (zalim),"
	},
	{
		"aya_id": 6055,
		"aya_number": 12,
		"aya_text": "اِذِ انْۢبَعَثَ اَشْقٰىهَاۖ",
		"sura_id": 91,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "ketika bangkit orang yang paling celaka di antara mereka,"
	},
	{
		"aya_id": 6056,
		"aya_number": 13,
		"aya_text": "فَقَالَ لَهُمْ رَسُوْلُ اللّٰهِ نَاقَةَ اللّٰهِ وَسُقْيٰهَاۗ",
		"sura_id": 91,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "lalu Rasul Allah (Saleh) berkata kepada mereka, “(Biarkanlah) unta betina dari Allah ini dengan minumannya.”"
	},
	{
		"aya_id": 6057,
		"aya_number": 14,
		"aya_text": "فَكَذَّبُوْهُ فَعَقَرُوْهَاۖ فَدَمْدَمَ عَلَيْهِمْ رَبُّهُمْ بِذَنْۢبِهِمْ فَسَوّٰىهَاۖ",
		"sura_id": 91,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "Namun mereka mendustakannya dan menyembelihnya, karena itu Tuhan membinasakan mereka karena dosanya, lalu diratakan-Nya (dengan tanah),"
	},
	{
		"aya_id": 6058,
		"aya_number": 15,
		"aya_text": "وَلَا يَخَافُ عُقْبٰهَا ࣖ",
		"sura_id": 91,
		"juz_id": 30,
		"page_number": 595,
		"translation_aya_text": "dan Dia tidak takut terhadap akibatnya."
	}
]
