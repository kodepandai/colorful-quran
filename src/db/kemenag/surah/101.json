[
	{
		"aya_id": 6158,
		"aya_number": 1,
		"aya_text": "اَلْقَارِعَةُۙ",
		"sura_id": 101,
		"juz_id": 30,
		"page_number": 600,
		"translation_aya_text": "Hari Kiamat,",
		"tajweed": []
	},
	{
		"aya_id": 6159,
		"aya_number": 2,
		"aya_text": "مَا الْقَارِعَةُ ۚ",
		"sura_id": 101,
		"juz_id": 30,
		"page_number": 600,
		"translation_aya_text": "Apakah hari Kiamat itu?",
		"tajweed": []
	},
	{
		"aya_id": 6160,
		"aya_number": 3,
		"aya_text": "وَمَآ اَدْرٰىكَ مَا الْقَارِعَةُ ۗ",
		"sura_id": 101,
		"juz_id": 30,
		"page_number": 600,
		"translation_aya_text": "Dan tahukah kamu apakah hari Kiamat itu?",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 4,
				"end": 7
			},
			{
				"class": "qalqalah",
				"start": 9,
				"end": 10
			}
		]
	},
	{
		"aya_id": 6161,
		"aya_number": 4,
		"aya_text": "يَوْمَ يَكُوْنُ النَّاسُ كَالْفَرَاشِ الْمَبْثُوْثِۙ",
		"sura_id": 101,
		"juz_id": 30,
		"page_number": 600,
		"translation_aya_text": "Pada hari itu manusia seperti laron yang beterbangan,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 18,
				"end": 21
			},
			{
				"class": "qalqalah",
				"start": 43,
				"end": 44
			},
			{
				"class": "madd-arid-lissukun",
				"start": 47,
				"end": 49
			}
		]
	},
	{
		"aya_id": 6162,
		"aya_number": 5,
		"aya_text": "وَتَكُوْنُ الْجِبَالُ كَالْعِهْنِ الْمَنْفُوْشِۗ",
		"sura_id": 101,
		"juz_id": 30,
		"page_number": 600,
		"translation_aya_text": "dan gunung-gunung seperti bulu yang dihambur-hamburkan.",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 39,
				"end": 43
			},
			{
				"class": "madd-arid-lissukun",
				"start": 43,
				"end": 45
			}
		]
	},
	{
		"aya_id": 6163,
		"aya_number": 6,
		"aya_text": "فَاَمَّا مَنْ ثَقُلَتْ مَوَازِينُهٗۙ",
		"sura_id": 101,
		"juz_id": 30,
		"page_number": 600,
		"translation_aya_text": "Maka adapun orang yang berat timbangan (kebaikan)nya,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			},
			{
				"class": "ikhfa",
				"start": 11,
				"end": 16
			}
		]
	},
	{
		"aya_id": 6164,
		"aya_number": 7,
		"aya_text": "فَهُوَ فِيْ عِيْشَةٍ رَّاضِيَةٍۗ",
		"sura_id": 101,
		"juz_id": 30,
		"page_number": 600,
		"translation_aya_text": "maka dia berada dalam kehidupan yang memuaskan (senang).",
		"tajweed": [
			{
				"class": "idgham-bilaghunnah",
				"start": 18,
				"end": 23
			}
		]
	},
	{
		"aya_id": 6165,
		"aya_number": 8,
		"aya_text": "وَاَمَّا مَنْ خَفَّتْ مَوَازِيْنُهٗۙ",
		"sura_id": 101,
		"juz_id": 30,
		"page_number": 600,
		"translation_aya_text": "Dan adapun orang yang ringan timbangan (kebaikan)nya,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			}
		]
	},
	{
		"aya_id": 6166,
		"aya_number": 9,
		"aya_text": "فَاُمُّهٗ هَاوِيَةٌ  ۗ",
		"sura_id": 101,
		"juz_id": 30,
		"page_number": 600,
		"translation_aya_text": "maka tempat kembalinya adalah neraka Hawiyah.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			}
		]
	},
	{
		"aya_id": 6167,
		"aya_number": 10,
		"aya_text": "وَمَآ اَدْرٰىكَ مَا هِيَهْۗ",
		"sura_id": 101,
		"juz_id": 30,
		"page_number": 600,
		"translation_aya_text": "Dan tahukah kamu apakah neraka Hawiyah itu?",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 4,
				"end": 7
			},
			{
				"class": "qalqalah",
				"start": 9,
				"end": 10
			}
		]
	},
	{
		"aya_id": 6168,
		"aya_number": 11,
		"aya_text": "نَارٌ حَامِيَةٌ ࣖ",
		"sura_id": 101,
		"juz_id": 30,
		"page_number": 600,
		"translation_aya_text": "(Yaitu) api yang sangat panas.",
		"tajweed": []
	}
]
