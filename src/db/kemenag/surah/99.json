[
	{
		"aya_id": 6139,
		"aya_number": 1,
		"aya_text": "اِذَا زُلْزِلَتِ الْاَرْضُ زِلْزَالَهَاۙ",
		"sura_id": 99,
		"juz_id": 30,
		"page_number": 599,
		"translation_aya_text": "Apabila bumi diguncangkan dengan guncangan yang dahsyat,",
		"tajweed": []
	},
	{
		"aya_id": 6140,
		"aya_number": 2,
		"aya_text": "وَاَخْرَجَتِ الْاَرْضُ اَثْقَالَهَاۙ",
		"sura_id": 99,
		"juz_id": 30,
		"page_number": 599,
		"translation_aya_text": "dan bumi telah mengeluarkan beban-beban berat (yang dikandung)nya,",
		"tajweed": []
	},
	{
		"aya_id": 6141,
		"aya_number": 3,
		"aya_text": "وَقَالَ الْاِنْسَانُ مَا لَهَاۚ",
		"sura_id": 99,
		"juz_id": 30,
		"page_number": 599,
		"translation_aya_text": "Dan manusia bertanya, “Apa yang terjadi pada bumi ini?”",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 13,
				"end": 17
			}
		]
	},
	{
		"aya_id": 6142,
		"aya_number": 4,
		"aya_text": "يَوْمَىِٕذٍ تُحَدِّثُ اَخْبَارَهَاۙ",
		"sura_id": 99,
		"juz_id": 30,
		"page_number": 599,
		"translation_aya_text": "Pada hari itu bumi menyampaikan beritanya,",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 9,
				"end": 14
			}
		]
	},
	{
		"aya_id": 6143,
		"aya_number": 5,
		"aya_text": "بِاَنَّ رَبَّكَ اَوْحٰى لَهَاۗ",
		"sura_id": 99,
		"juz_id": 30,
		"page_number": 599,
		"translation_aya_text": "karena sesungguhnya Tuhanmu telah memerintahkan (yang sedemikian itu) padanya.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 6
			}
		]
	},
	{
		"aya_id": 6144,
		"aya_number": 6,
		"aya_text": "يَوْمَىِٕذٍ يَّصْدُرُ النَّاسُ اَشْتَاتًا ەۙ لِّيُرَوْا اَعْمَالَهُمْۗ",
		"sura_id": 99,
		"juz_id": 30,
		"page_number": 599,
		"translation_aya_text": "Pada hari itu manusia keluar dari kuburnya dalam keadaan berkelompok-kelompok, untuk diperlihatkan kepada mereka (balasan) semua perbuatannya.",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 9,
				"end": 14
			},
			{
				"class": "ghunnah",
				"start": 24,
				"end": 26
			},
			{
				"class": "idgham-bilaghunnah",
