[
	{
		"aya_id": 6147,
		"aya_number": 1,
		"aya_text": "وَالْعٰدِيٰتِ ضَبْحًاۙ ",
		"sura_id": 100,
		"juz_id": 30,
		"page_number": 599,
		"translation_aya_text": "Demi kuda perang yang berlari kencang terengah-engah,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 16,
				"end": 17
			}
		]
	},
	{
		"aya_id": 6148,
		"aya_number": 2,
		"aya_text": "فَالْمُوْرِيٰتِ قَدْحًاۙ",
		"sura_id": 100,
		"juz_id": 30,
		"page_number": 599,
		"translation_aya_text": "dan kuda yang memercikkan bunga api (dengan pukulan kuku kakinya),",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 18,
				"end": 19
			}
		]
	},
	{
		"aya_id": 6149,
		"aya_number": 3,
		"aya_text": "فَالْمُغِيْرٰتِ صُبْحًاۙ",
		"sura_id": 100,
		"juz_id": 30,
		"page_number": 599,
		"translation_aya_text": "dan kuda yang menyerang (dengan tiba-tiba) pada waktu pagi,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 18,
				"end": 19
			}
		]
	},
	{
		"aya_id": 6150,
		"aya_number": 4,
		"aya_text": "فَاَثَرْنَ بِهٖ نَقْعًاۙ",
		"sura_id": 100,
		"juz_id": 30,
		"page_number": 599,
		"translation_aya_text": "sehingga menerbangkan debu,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 18,
				"end": 19
			}
		]
	},
	{
		"aya_id": 6151,
		"aya_number": 5,
		"aya_text": "فَوَسَطْنَ بِهٖ جَمْعًاۙ",
		"sura_id": 100,
		"juz_id": 30,
		"page_number": 599,
		"translation_aya_text": "lalu menyerbu ke tengah-tengah kumpulan musuh,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 6,
				"end": 7
			}
		]
	},
	{
		"aya_id": 6152,
		"aya_number": 6,
		"aya_text": "اِنَّ الْاِنْسَانَ لِرَبِّهٖ لَكَنُوْدٌ ۚ",
		"sura_id": 100,
		"juz_id": 30,
		"page_number": 599,
		"translation_aya_text": "sungguh, manusia itu sangat ingkar, (tidak bersyukur) kepada Tuhannya,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			},
			{
				"class": "ikhfa",
				"start": 11,
				"end": 15
			},
			{
				"class": "madd-arid-lissukun",
				"start": 35,
				"end": 37
			}
		]
	},
	{
		"aya_id": 6153,
		"aya_number": 7,
		"aya_text": "وَاِنَّهٗ عَلٰى ذٰلِكَ لَشَهِيْدٌۚ",
		"sura_id": 100,
		"juz_id": 30,
		"page_number": 599,
		"translation_aya_text": "dan sesungguhnya dia (manusia) menyaksikan (mengakui) keingkarannya,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			},
			{
				"class": "madd-arid-lissukun",
				"start": 29,
				"end": 31
			}
		]
	},
	{
		"aya_id": 6154,
		"aya_number": 8,
		"aya_text": "وَاِنَّهٗ لِحُبِّ الْخَيْرِ لَشَدِيْدٌ ۗ",
		"sura_id": 100,
		"juz_id": 30,
		"page_number": 599,
		"translation_aya_text": "dan sesungguhnya cintanya kepada harta benar-benar berlebihan.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 4,
				"end": 7
			},
			{
				"class": "madd-arid-lissukun",
				"start": 34,
				"end": 36
			}
		]
	},
	{
		"aya_id": 6155,
		"aya_number": 9,
		"aya_text": "۞ اَفَلَا يَعْلَمُ اِذَا بُعْثِرَ مَا فِى الْقُبُوْرِۙ",
		"sura_id": 100,
		"juz_id": 30,
		"page_number": 599,
		"translation_aya_text": "Maka tidakkah dia mengetahui apabila apa yang di dalam kubur dikeluarkan,",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 49,
				"end": 51
			}
		]
	},
	{
		"aya_id": 6156,
		"aya_number": 10,
		"aya_text": "وَحُصِّلَ مَا فِى الصُّدُوْرِۙ",
		"sura_id": 100,
		"juz_id": 30,
		"page_number": 600,
		"translation_aya_text": "dan apa yang tersimpan di dalam dada dilahirkan?",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 25,
				"end": 27
			}
		]
	},
	{
		"aya_id": 6157,
		"aya_number": 11,
		"aya_text": "اِنَّ رَبَّهُمْ بِهِمْ يَوْمَىِٕذٍ لَّخَبِيْرٌ ࣖ",
		"sura_id": 100,
		"juz_id": 30,
		"page_number": 600,
		"translation_aya_text": "sungguh, Tuhan mereka pada hari itu Mahateliti terhadap keadaan mereka.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			},
			{
				"class": "idgham-bilaghunnah",
