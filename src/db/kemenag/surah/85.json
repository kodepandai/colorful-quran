[
	{
		"aya_id": 5910,
		"aya_number": 1,
		"aya_text": "وَالسَّمَاۤءِ ذَاتِ الْبُرُوْجِۙ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "Demi langit yang mempunyai gugusan bintang,",
		"tajweed": [
			{
				"class": "madd-wajib",
				"start": 9,
				"end": 11
			},
			{
				"class": "madd-arid-lissukun",
				"start": 27,
				"end": 29
			}
		]
	},
	{
		"aya_id": 5911,
		"aya_number": 2,
		"aya_text": "وَالْيَوْمِ الْمَوْعُوْدِۙ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "dan demi hari yang dijanjikan.",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 21,
				"end": 23
			}
		]
	},
	{
		"aya_id": 5912,
		"aya_number": 3,
		"aya_text": "وَشَاهِدٍ وَّمَشْهُوْدٍۗ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "Demi yang menyaksikan dan yang disaksikan.",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 7,
				"end": 12
			},
			{
				"class": "madd-arid-lissukun",
				"start": 19,
				"end": 21
			}
		]
	},
	{
		"aya_id": 5913,
		"aya_number": 4,
		"aya_text": "قُتِلَ اَصْحٰبُ الْاُخْدُوْدِۙ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "Binasalah orang-orang yang membuat parit (yaitu para pembesar Najran di Yaman),",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 25,
				"end": 27
			}
		]
	},
	{
		"aya_id": 5914,
		"aya_number": 5,
		"aya_text": "النَّارِ ذَاتِ الْوَقُوْدِۙ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "yang berapi (yang mempunyai) kayu bakar,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "madd-arid-lissukun",
				"start": 22,
				"end": 24
			}
		]
	},
	{
		"aya_id": 5915,
		"aya_number": 6,
		"aya_text": "اِذْ هُمْ عَلَيْهَا قُعُوْدٌۙ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "ketika mereka duduk di sekitarnya, ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 24,
				"end": 26
			}
		]
	},
	{
		"aya_id": 5916,
		"aya_number": 7,
		"aya_text": "وَّهُمْ عَلٰى مَا يَفْعَلُوْنَ بِالْمُؤْمِنِيْنَ شُهُوْدٌ  ۗ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "sedang mereka menyaksikan apa yang mereka perbuat terhadap orang-orang mukmin.",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 53,
				"end": 55
			}
		]
	},
	{
		"aya_id": 5917,
		"aya_number": 8,
		"aya_text": "وَمَا نَقَمُوْا مِنْهُمْ اِلَّآ اَنْ يُّؤْمِنُوْا بِاللّٰهِ الْعَزِيْزِ الْحَمِيْدِۙ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "Dan mereka menyiksa orang-orang mukmin itu hanya karena (orang-orang mukmin itu) beriman kepada Allah Yang Mahaperkasa, Maha Terpuji,",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 30,
				"end": 33
			},
			{
				"class": "idgham-bighunnah",
				"start": 35,
				"end": 40
			},
			{
				"class": "madd-arid-lissukun",
				"start": 80,
				"end": 82
			}
		]
	},
	{
		"aya_id": 5918,
		"aya_number": 9,
		"aya_text": "الَّذِيْ لَهٗ مُلْكُ السَّمٰوٰتِ وَالْاَرْضِ ۗوَاللّٰهُ عَلٰى كُلِّ شَيْءٍ شَهِيْدٌ  ۗ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "yang memiliki kerajaan langit dan bumi. Dan Allah Maha Menyaksikan segala sesuatu.",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 72,
				"end": 77
			},
			{
				"class": "madd-arid-lissukun",
				"start": 79,
				"end": 81
			}
		]
	},
	{
		"aya_id": 5919,
		"aya_number": 10,
		"aya_text": "اِنَّ الَّذِيْنَ فَتَنُوا الْمُؤْمِنِيْنَ وَالْمُؤْمِنٰتِ ثُمَّ لَمْ يَتُوْبُوْا فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ الْحَرِيْقِۗ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "Sungguh, orang-orang yang mendatangkan cobaan (bencana, membunuh, menyiksa) kepada orang-orang mukmin laki-laki dan perempuan lalu mereka tidak bertobat, maka mereka akan mendapat azab Jahanam dan mereka akan mendapat azab (neraka) yang membakar.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "ghunnah",
				"start": 60,
				"end": 62
			},
			{
				"class": "ghunnah",
				"start": 102,
				"end": 104
			},
			{
				"class": "madd-arid-lissukun",
				"start": 132,
				"end": 134
			}
		]
	},
	{
		"aya_id": 5920,
		"aya_number": 11,
		"aya_text": "اِنَّ الَّذِيْنَ اٰمَنُوْا وَعَمِلُوا الصّٰلِحٰتِ لَهُمْ جَنّٰتٌ تَجْرِيْ مِنْ تَحْتِهَا الْاَنْهٰرُ ەۗ ذٰلِكَ الْفَوْزُ الْكَبِيْرُۗ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "Sungguh, orang-orang yang beriman dan mengerjakan kebajikan, mereka akan mendapat surga yang mengalir di bawahnya sungai-sungai, itulah kemenangan yang agung.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "ghunnah",
				"start": 59,
				"end": 61
			},
			{
				"class": "ikhfa",
				"start": 62,
				"end": 67
			},
			{
				"class": "qalqalah",
				"start": 67,
				"end": 68
			},
			{
				"class": "ikhfa",
				"start": 76,
				"end": 81
			},
			{
				"class": "madd-arid-lissukun",
				"start": 128,
				"end": 130
			}
		]
	},
	{
		"aya_id": 5921,
		"aya_number": 12,
		"aya_text": "اِنَّ بَطْشَ رَبِّكَ لَشَدِيْدٌ ۗ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "Sungguh, azab Tuhanmu sangat keras.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "qalqalah",
				"start": 8,
				"end": 9
			},
			{
				"class": "madd-arid-lissukun",
				"start": 27,
				"end": 29
			}
		]
	},
	{
		"aya_id": 5922,
		"aya_number": 13,
		"aya_text": "اِنَّهٗ هُوَ يُبْدِئُ وَيُعِيْدُۚ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "Sungguh, Dialah yang memulai pen-ciptaan (makhluk) dan yang menghidupkannya (kembali).",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 4
			},
			{
				"class": "qalqalah",
				"start": 15,
				"end": 16
			},
			{
				"class": "madd-arid-lissukun",
				"start": 28,
				"end": 30
			}
		]
	},
	{
		"aya_id": 5923,
		"aya_number": 14,
		"aya_text": "وَهُوَ الْغَفُوْرُ الْوَدُوْدُۙ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "Dan Dialah Yang Maha Pengampun, Maha Pengasih,",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 26,
				"end": 28
			}
		]
	},
	{
		"aya_id": 5924,
		"aya_number": 15,
		"aya_text": "ذُو الْعَرْشِ الْمَجِيْدُۙ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "yang memiliki ‘Arsy, lagi Mahamulia, ",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 21,
				"end": 23
			}
		]
	},
	{
		"aya_id": 5925,
		"aya_number": 16,
		"aya_text": "فَعَّالٌ لِّمَا يُرِيْدُۗ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "Mahakuasa berbuat apa yang Dia kehendaki. ",
		"tajweed": [
			{
				"class": "idgham-bilaghunnah",
				"start": 6,
				"end": 11
			},
			{
				"class": "madd-arid-lissukun",
				"start": 20,
				"end": 22
			}
		]
	},
	{
		"aya_id": 5926,
		"aya_number": 17,
		"aya_text": "هَلْ اَتٰىكَ حَدِيْثُ الْجُنُوْدِۙ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "Sudahkah sampai kepadamu berita tentang bala tentara (penentang),",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 29,
				"end": 31
			}
		]
	},
	{
		"aya_id": 5927,
		"aya_number": 18,
		"aya_text": "فِرْعَوْنَ وَثَمُوْدَۗ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "(yaitu) Fir‘aun dan Samud?",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 17,
				"end": 19
			}
		]
	},
	{
		"aya_id": 5928,
		"aya_number": 19,
		"aya_text": "بَلِ الَّذِيْنَ كَفَرُوْا فِيْ تَكْذِيْبٍۙ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "Memang orang-orang kafir (selalu) mendustakan,",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 37,
				"end": 39
			}
		]
	},
	{
		"aya_id": 5929,
		"aya_number": 20,
		"aya_text": "وَّاللّٰهُ مِنْ وَّرَاۤىِٕهِمْ مُّحِيْطٌۚ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "padahal Allah mengepung dari belakang mereka (sehingga tidak dapat lolos).",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 13,
				"end": 18
			},
			{
				"class": "madd-wajib",
				"start": 21,
				"end": 24
			},
			{
				"class": "idgham-mimi",
				"start": 28,
				"end": 33
			},
			{
				"class": "madd-arid-lissukun",
				"start": 36,
				"end": 38
			}
		]
	},
	{
		"aya_id": 5930,
		"aya_number": 21,
		"aya_text": "بَلْ هُوَ قُرْاٰنٌ مَّجِيْدٌۙ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "Bahkan (yang didustakan itu) ialah Al-Qur'an yang mulia,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 16,
				"end": 21
			},
			{
				"class": "madd-arid-lissukun",
				"start": 24,
				"end": 26
			}
		]
	},
	{
		"aya_id": 5931,
		"aya_number": 22,
		"aya_text": "فِيْ لَوْحٍ مَّحْفُوْظٍ ࣖ ",
		"sura_id": 85,
		"juz_id": 30,
		"page_number": 590,
		"translation_aya_text": "yang (tersimpan) dalam (tempat) yang terjaga (Lauh Mahfuzh).",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 9,
				"end": 14
			},
			{
				"class": "madd-arid-lissukun",
				"start": 19,
				"end": 21
			}
		]
	}
]
