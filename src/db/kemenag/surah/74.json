[
	{
		"aya_id": 5496,
		"aya_number": 1,
		"aya_text": "يٰٓاَيُّهَا الْمُدَّثِّرُۙ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "Wahai orang yang berkemul (berselimut)!",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 0,
				"end": 3
			}
		]
	},
	{
		"aya_id": 5497,
		"aya_number": 2,
		"aya_text": "قُمْ فَاَنْذِرْۖ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "bangunlah, lalu berilah peringatan!",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 9,
				"end": 13
			}
		]
	},
	{
		"aya_id": 5498,
		"aya_number": 3,
		"aya_text": "وَرَبَّكَ فَكَبِّرْۖ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "dan agungkanlah Tuhanmu,",
		"tajweed": []
	},
	{
		"aya_id": 5499,
		"aya_number": 4,
		"aya_text": "وَثِيَابَكَ فَطَهِّرْۖ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "dan bersihkanlah pakaianmu,",
		"tajweed": []
	},
	{
		"aya_id": 5500,
		"aya_number": 5,
		"aya_text": "وَالرُّجْزَ فَاهْجُرْۖ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "dan tinggalkanlah segala (perbuatan) yang keji,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 7,
				"end": 8
			}
		]
	},
	{
		"aya_id": 5501,
		"aya_number": 6,
		"aya_text": "وَلَا تَمْنُنْ تَسْتَكْثِرُۖ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "dan janganlah engkau (Muhammad) memberi (dengan maksud) memperoleh (balasan) yang lebih banyak.",
		"tajweed": [
			{
				"class": "ikhfa",
				"start": 12,
				"end": 17
			}
		]
	},
	{
		"aya_id": 5502,
		"aya_number": 7,
		"aya_text": "وَلِرَبِّكَ فَاصْبِرْۗ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "Dan karena Tuhanmu, bersabarlah.",
		"tajweed": []
	},
	{
		"aya_id": 5503,
		"aya_number": 8,
		"aya_text": "فَاِذَا نُقِرَ فِى النَّاقُوْرِۙ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "Maka apabila sangkakala ditiup,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 21,
				"end": 24
			},
			{
				"class": "madd-arid-lissukun",
				"start": 27,
				"end": 29
			}
		]
	},
	{
		"aya_id": 5504,
		"aya_number": 9,
		"aya_text": "فَذٰلِكَ يَوْمَىِٕذٍ يَّوْمٌ عَسِيْرٌۙ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "maka itulah hari yang serba sulit,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 18,
				"end": 23
			},
			{
				"class": "madd-arid-lissukun",
				"start": 33,
				"end": 35
			}
		]
	},
	{
		"aya_id": 5505,
		"aya_number": 10,
		"aya_text": "عَلَى الْكٰفِرِيْنَ غَيْرُ يَسِيْرٍ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "bagi orang-orang kafir tidak mudah.",
		"tajweed": [
			{
				"class": "madd-arid-lissukun",
				"start": 31,
				"end": 33
			}
		]
	},
	{
		"aya_id": 5506,
		"aya_number": 11,
		"aya_text": "ذَرْنِيْ وَمَنْ خَلَقْتُ وَحِيْدًاۙ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "Biarkanlah Aku (yang bertindak) terhadap orang yang Aku sendiri telah menciptakannya,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 20,
				"end": 21
			}
		]
	},
	{
		"aya_id": 5507,
		"aya_number": 12,
		"aya_text": "وَّجَعَلْتُ لَهٗ مَالًا مَّمْدُوْدًاۙ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "dan Aku beri kekayaan yang melimpah,",
		"tajweed": [
			{
				"class": "idgham-bighunnah",
				"start": 20,
				"end": 22
			},
			{
				"class": "idgham-bighunnah",
				"start": 23,
				"end": 25
			}
		]
	},
	{
		"aya_id": 5508,
		"aya_number": 13,
		"aya_text": "وَّبَنِيْنَ شُهُوْدًاۙ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "dan anak-anak yang selalu bersamanya,",
		"tajweed": []
	},
	{
		"aya_id": 5509,
		"aya_number": 14,
		"aya_text": "وَّمَهَّدْتُّ لَهٗ تَمْهِيْدًاۙ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "dan Aku beri kelapangan (hidup) seluas-luasnya.",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 8,
				"end": 9
			}
		]
	},
	{
		"aya_id": 5510,
		"aya_number": 15,
		"aya_text": "ثُمَّ يَطْمَعُ اَنْ اَزِيْدَۙ    ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "Kemudian dia ingin sekali agar Aku menambahnya.",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			},
			{
				"class": "qalqalah",
				"start": 8,
				"end": 9
			},
			{
				"class": "madd-arid-lissukun",
				"start": 24,
				"end": 26
			}
		]
	},
	{
		"aya_id": 5511,
		"aya_number": 16,
		"aya_text": "كَلَّاۗ اِنَّهٗ كَانَ لِاٰيٰتِنَا عَنِيْدًاۗ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "Tidak bisa! Sesungguhnya dia telah menentang ayat-ayat Kami (Al-Qur'an).",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 10,
				"end": 13
			}
		]
	},
	{
		"aya_id": 5512,
		"aya_number": 17,
		"aya_text": "سَاُرْهِقُهٗ صَعُوْدًاۗ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 575,
		"translation_aya_text": "Aku akan membebaninya dengan pendakian yang memayahkan.",
		"tajweed": []
	},
	{
		"aya_id": 5513,
		"aya_number": 18,
		"aya_text": "اِنَّهٗ فَكَّرَ وَقَدَّرَۙ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 576,
		"translation_aya_text": "Sesungguhnya dia telah memikirkan dan menetapkan (apa yang ditetapkannya),",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			}
		]
	},
	{
		"aya_id": 5514,
		"aya_number": 19,
		"aya_text": "فَقُتِلَ كَيْفَ قَدَّرَۙ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 576,
		"translation_aya_text": "maka celakalah dia! Bagaimana dia menetapkan?",
		"tajweed": []
	},
	{
		"aya_id": 5515,
		"aya_number": 20,
		"aya_text": "ثُمَّ قُتِلَ كَيْفَ قَدَّرَۙ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 576,
		"translation_aya_text": "Sekali lagi, celakalah dia! Bagaimana dia menetapkan?",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			}
		]
	},
	{
		"aya_id": 5516,
		"aya_number": 21,
		"aya_text": "ثُمَّ نَظَرَۙ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 576,
		"translation_aya_text": "Kemudian dia (merenung) memikirkan,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			}
		]
	},
	{
		"aya_id": 5517,
		"aya_number": 22,
		"aya_text": "ثُمَّ عَبَسَ وَبَسَرَۙ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 576,
		"translation_aya_text": "lalu berwajah masam dan cemberut,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			}
		]
	},
	{
		"aya_id": 5518,
		"aya_number": 23,
		"aya_text": "ثُمَّ اَدْبَرَ وَاسْتَكْبَرَۙ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 576,
		"translation_aya_text": "kemudian berpaling (dari kebenaran) dan menyombongkan diri,",
		"tajweed": [
			{
				"class": "ghunnah",
				"start": 2,
				"end": 5
			},
			{
				"class": "qalqalah",
				"start": 8,
				"end": 9
			}
		]
	},
	{
		"aya_id": 5519,
		"aya_number": 24,
		"aya_text": "فَقَالَ اِنْ هٰذَآ اِلَّا سِحْرٌ يُّؤْثَرُۙ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 576,
		"translation_aya_text": "lalu dia berkata, “(Al-Qur'an) ini hanyalah sihir yang dipelajari (dari orang-orang dahulu).",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 17,
				"end": 20
			},
			{
				"class": "idgham-bighunnah",
				"start": 31,
				"end": 36
			}
		]
	},
	{
		"aya_id": 5520,
		"aya_number": 25,
		"aya_text": "اِنْ هٰذَآ اِلَّا قَوْلُ الْبَشَرِۗ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 576,
		"translation_aya_text": "Ini hanyalah perkataan manusia.”",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 9,
				"end": 12
			}
		]
	},
	{
		"aya_id": 5521,
		"aya_number": 26,
		"aya_text": "سَاُصْلِيْهِ سَقَرَ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 576,
		"translation_aya_text": "Kelak, Aku akan memasukkannya ke dalam (neraka) Saqar,",
		"tajweed": []
	},
	{
		"aya_id": 5522,
		"aya_number": 27,
		"aya_text": "وَمَآ اَدْرٰىكَ مَا سَقَرُۗ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 576,
		"translation_aya_text": "dan tahukah kamu apa (neraka) Saqar itu?",
		"tajweed": [
			{
				"class": "madd-jaiz",
				"start": 4,
				"end": 7
			},
			{
				"class": "qalqalah",
				"start": 9,
				"end": 10
			}
		]
	},
	{
		"aya_id": 5523,
		"aya_number": 28,
		"aya_text": "لَا تُبْقِيْ وَلَا تَذَرُۚ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 576,
		"translation_aya_text": "Ia (Saqar itu) tidak meninggalkan dan tidak membiarkan,",
		"tajweed": [
			{
				"class": "qalqalah",
				"start": 6,
				"end": 7
			}
		]
	},
	{
		"aya_id": 5524,
		"aya_number": 29,
		"aya_text": "لَوَّاحَةٌ لِّلْبَشَرِۚ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 576,
		"translation_aya_text": "yang menghanguskan kulit manusia.",
		"tajweed": [
			{
				"class": "idgham-bilaghunnah",
				"start": 8,
				"end": 13
			}
		]
	},
	{
		"aya_id": 5525,
		"aya_number": 30,
		"aya_text": "عَلَيْهَا تِسْعَةَ عَشَرَۗ ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 576,
		"translation_aya_text": "Di atasnya ada sembilan belas (malaikat penjaga).",
		"tajweed": []
	},
	{
		"aya_id": 5526,
		"aya_number": 31,
		"aya_text": "وَمَا جَعَلْنَآ اَصْحٰبَ النَّارِ اِلَّا مَلٰۤىِٕكَةً ۖوَّمَا جَعَلْنَا عِدَّتَهُمْ اِلَّا فِتْنَةً لِّلَّذِيْنَ كَفَرُوْاۙ لِيَسْتَيْقِنَ الَّذِيْنَ اُوْتُوا الْكِتٰبَ وَيَزْدَادَ الَّذِيْنَ اٰمَنُوْٓا اِيْمَانًا وَّلَا يَرْتَابَ الَّذِيْنَ اُوْتُوا الْكِتٰبَ وَالْمُؤْمِنُوْنَۙ وَلِيَقُوْلَ الَّذِيْنَ فِيْ قُلُوْبِهِمْ مَّرَضٌ وَّالْكٰفِرُوْنَ مَاذَآ اَرَادَ اللّٰهُ بِهٰذَا مَثَلًاۗ  كَذٰلِكَ يُضِلُّ اللّٰهُ مَنْ يَّشَاۤءُ وَيَهْدِيْ مَنْ يَّشَاۤءُۗ وَمَا يَعْلَمُ جُنُوْدَ رَبِّكَ اِلَّا هُوَۗ وَمَا هِيَ اِلَّا ذِكْرٰى لِلْبَشَرِ ࣖ",
		"sura_id": 74,
		"juz_id": 29,
		"page_number": 576,
		"translation_aya_text": "Dan yang Kami jadikan penjaga neraka itu hanya dari malaikat; dan Kami menentukan bilangan mereka itu hanya sebagai cobaan bagi orang-orang kafir, agar orang-orang yang diberi kitab menjadi yakin, agar orang yang beriman bertambah imannya, agar orang-orang yang diberi kitab dan orang-orang mukmin itu tidak ragu-ragu; dan agar orang-orang yang di dalam hatinya ada penyakit dan orang-orang kafir (berkata), “Apakah yang dikehendaki Allah dengan (bilangan) ini sebagai suatu perumpamaan?” Demikianlah Allah membiarkan sesat orang-orang yang Dia kehendaki dan memberi petunjuk kepada orang-orang yang Dia kehendaki. Dan tidak ada yang mengetahui bala tentara Tuhanmu kecuali Dia sendiri. Dan Saqar itu tidak lain hanyalah peringatan bagi manusia.",
		"tajweed": [
